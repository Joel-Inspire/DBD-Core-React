0010 REM "WWW Send a file to the output <WWWSND>
0020 SETESC 9300; SETERR 9000
0035 REM "4.0 - 08/29/96 - 10.99 - kmc
0040 REM "Copyright 1996 Computer Software Inc.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0090 CLEAR ; SETERR 0100; ENTER PARAM$,FILE_NAME$,OUT_FILE
0100 SETERR 9000
0110 X0$="WWWSND",X1$="WWW Send file"
0120 EOL$=STP(PARAM$(1,4),1)
3000 SEND_FILE:REM "Put contents of FILE_NAME$ to OUTPUT
3010 IN_FILE=UNT; OPEN LOCK (IN_FILE,OPT="TEXT",ERR=SEND_FILE_END)FILE_NAME$
3020 READ (IN_FILE,END=3021)F_IN$; PRINT (OUT_FILE)F_IN$,EOL$,; GOTO 3020
3040 CLOSE (IN_FILE)
3045 GOTO 9900
9000 REM "ERROR PROCESSING
9005 PRINT (OUT_FILE,ERR=9010)"ERR=",ERR," LINE = ",TCB(5),'LF','LF'
9010 GOTO 9900
9900 REM "End
9950 EXIT 
9999 END 
