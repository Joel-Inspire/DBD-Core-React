0010 REM "Maintain check invoice G/L cash basis records <AP2UT2>
0020 SETESC 9300; SETERR 9000
0040 REM "Copyright 1987 Computer Software Inc.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0090 ON L0 GOTO 0100,6700
0100 BEGIN ; SETERR 9000
0110 X0$="AP2UT2",X1$="Check inv. G/L cash basis"
0120 DIM B$(46),T[9]
0130 K0=18,K1=2
0135 C9=-1,V9=-2
0140 M0$="-####0",M1$="-#,###,###.00",M2$="-###,###.00"
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=MNM('GS')+Z0$+MNM('GE')
0300 REM "IOLISTS
0310 IOLIST A$(1,LEN(A$)),A[0],A[1],A[2],A[3],A[4],A[5],A[6],A[7]
0320 IOLIST B$
0330 IOLIST C$,C[0],C[1],C[2],C[3],C[4],C[5],C[6],C[7]
0340 IOLIST D$
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01X APN...  02O AP4...  03O APO...  04O GL1...  05O ZYB...  06O ZZPARM  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0560 READ (Z[6],KEY=X3$(9,3)+"A/P",DOM=9800)F0$
0565 DIM F1$(110); READ (Z[6],KEY=X3$(9,3)+"AP2A",DOM=0566)F1$
0600 REM "
0610 GOSUB 6000
0620 GOSUB 7800
1000 REM "
1010 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,X5$,X0,X1,X2
1030 DIM B$(391),B[15],C$(54),C[3],D$(40)
1035 DIM A$(42+K9),A[14]
1045 A$(1,10+K9)=K9$+A1$
1060 PRINT (0,ERR=1070)'CF',; GOSUB 6450; GOTO 1100
1070 GOSUB 6000; GOSUB 6450
1100 REM "KEY SEQ
1110 X0=-1,C0=0,X1=0,C9=0,Z9=0
1120 C0=ABS(C0),X$=""; IF C0=2 AND X0>0 THEN X0=X0-1 ELSE IF C0<>2 THEN X0=X0+1; IF C0=4 THEN GOTO 9900
1140 ON X0 GOTO 1200,1250,1300,1350,1400,1450,1500
1200 REM "Fiscal Year
1205 IF LEN(A1$)>0 THEN A$(1,4)=A1$(1,4); GOTO 1212
1207 Q$=X3$(9,3)+"G/LYE"
1210 CALL "ZZENTR","ZXUX",A{ALL},A$,X4$,X3$,14,3+V0,1+K9,4,C0,"","{1"+X$,Q$,"AP2UT200","GLYE","",""; IF ABS(C0)>4 THEN GOSUB 6400; ON C0 GOTO 1210,1211
1215 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1216,6800,9800,6970
1220 FIND (Z[6],KEY=X3$(9,3)+"G/LYE"+A$(1,4),DOM=1210)P1$
1223 IF POS(P1$(99,1)="CR")<>0 THEN CALL "ZZPROM",".4",X3$,Z,"Closed Years must be reopened by System Administrator prior to posting.","","",0; GOTO 1210
1240 GOTO 1120
1250 REM "Accounting period
1254 IF ABS(C0)=2 THEN A1$=""; GOTO 1120
1255 IF LEN(A1$)>4 THEN A$(5,2)=A1$(5,2); GOTO 1262
1260 CALL "ZZENTR","ZUX",A{ALL},A$,X4$,X3$,43,3+V0,5+K9,2,C0,"","{2"+X$,"","AP2UT202","","",""
1262 IF ABS(C0)=2 AND LEN(A1$)>0 THEN A1$=A1$(1,0)
1265 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1266,6800,9800,6970
1267 IF A$(5+K9,2)<"01" OR A$(5+K9,2)>P1$(13,2) THEN GOTO 1260
1270 GOSUB 7700
1290 GOTO 1120
1300 REM "Bank code
1305 IF LEN(A1$)>6 THEN A$(7,3)=A1$(7,3); GOTO 1312 ELSE IF F0$(16,1)<>"Y" THEN A$(7,3)=F0$(17,3); PRINT @(14,4+V0),A$(7,3),; GOTO 1312
1310 CALL "ZZENTR","SZUX",A{ALL},A$,X4$,X3$,14,4+V0,7+K9,3,C0,"",X$,"","AP2UT204","ZYB","ZY2BAA",""; IF ABS(C0)>4 THEN GOSUB 6400; ON C0 GOTO 1310,1311
1312 IF ABS(C0)=2 AND LEN(A1$)>4 THEN A1$=A1$(1,4)
1320 FIND (Z[5],KEY=A$(7+K9,3),DOM=1310)IOL=0320
1325 IF F0$(16,1)="Y" THEN PRINT @(43,4+V0),B$(4,30),
1340 GOTO 1120
1350 REM "Check number
1355 IF LEN(A1$)>10 THEN A$(10,6)=A1$(10,6); PRINT @(14,5),A$(10,6),; GOTO 1365
1360 CALL "ZZENTR","ZX",A{ALL},A$,X4$,X3$,14,5,10,6,C0,"",X$,"","AP2UT206","","",""
1362 IF ABS(C0)=2 AND LEN(A1$)>6 THEN A1$=A1$(1,6)
1365 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1366,6800,9800,6970
1390 GOTO 1120
1400 REM "Sequence number
1405 IF LEN(A1$)>16 THEN A$(16,2)=A1$(16,2); PRINT @(14,6),A$(16,2),; GOTO 1416
1410 CALL "ZZENTR","SX",A{ALL},A$,X4$,X3$,14,6,16,2,C0,"",X$,"","AP2UT208","","",""
1412 IF ABS(C0)=2 AND LEN(A1$)>9 THEN A1$=A1$(1,9)
1415 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1416,6800,9800,6970
1440 GOTO 1120
1450 REM "Invoice number
1455 J$=A$(1,17)
1460 CALL "ZZENTR","SX",A{ALL},A$,X4$,X3$,14,7,18,10,C0,"",X$,J$,"AP2UT210","APN","",""
1462 IF ABS(C0)=2 AND LEN(A1$)>15 THEN A1$=A1$(1,15)
1465 IF C0=-1 THEN GOTO 1460 ELSE IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1466,6800,9800,6970
1490 GOTO 1120
1500 REM "
1505 Z9=-1
1510 A1$=K9$+A$(LEN(K9$)+1,27),A$(1,LEN(K9$))=K9$
1520 READ (Z[1],KEY=A1$,DOM=1600)IOL=0310
1525 Z9=0
1590 GOTO 1700
1600 REM "
1610 CALL "ZZPROM",".4",X3$,Z,"This invoice is not on file","","",0; X0=1; GOTO 1140
1620 CALL "ZZPROM","2",X3$,Z,"","","",0; ON Z GOTO 1650,1630,6975
1700 REM "
1710 GOSUB 6220
1750 CALL "ZZPROM","C",X3$,Z,"","","",0; ON Z GOTO 1000,1751
1800 REM "Calculate balance
1810 T[0]=-A[1],I0=A[0]
1820 IF I0<=0 THEN GOTO 1890
1825 DIM C$(60),C[8]
1830 READ (Z[3],IND=I0)I0,IOL=0330
1840 T[0]=T[0]+C[3]
1850 GOTO 1820
2000 REM "Maintain lines
2010 F=Z[3],V1=0,V3=Z[4],V9=-1
2020 A0=A[0],V6=28
2030 I9$=A$(1,27)
2050 RUN "GL2XUT"
6000 REM "BACKGROUND
6005 PRINT (0,ERR=6016)'SB',
6060 PRINT @(1,3),"Fiscal year:",@(24,3),"Accounting period:",@(3,4),"Bank code:",@(0,5),"Check number:",@(2,6),"Sequence #:",@(3,7),"Invoice #:",@(32,7),"Net amount:",
6075 PRINT @(0,8),Z0$,
6080 PRINT 'SF',
6090 RETURN 
6200 REM "DISPLAY DATA
6215 GOSUB 6450
6220 PRINT @(44,7),A[1]:M1$,
6290 RETURN 
6400 REM "WHOLE SCREEN
6405 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,X5$,X0,X1,0
6420 GOSUB 6000
6430 IF C9>=0 THEN GOSUB 6200 ELSE GOSUB 6450
6440 IF ABS(C0)>4 THEN C0=ABS(C0)-5
6445 RETURN 
6450 REM "DISPLAY KEYS
6455 IF C9<0 THEN GOTO 6445
6460 PRINT @(14,3),A$(1,4),@(43,3),A$(5,2),@(14,4),A$(7,3),@(14,5),A$(10,6),@(14,6),A$(16,2),@(14,7),A$(18,10),
6490 RETURN 
6500 REM "DELETE
6510 REMOVE (Z[1],KEY=A1$,DOM=6511)
6520 REMOVE (Z[7],KEY=A$(1+K9,10)+"A"+A$(20+K9,6)+A$(11+K9,8),DOM=6521)
6530 REMOVE (Z[7],KEY=A$(1+K9,10)+"B"+A$(20+K9,6)+A$(11+K9,8),DOM=6531)
6590 GOTO 1630
6600 REM "ALT KEY DATA READS
6605 FIND (Z[5],KEY=A$(7+K9,3),DOM=6606)IOL=0320
6690 RETURN 
6700 REM "ADD ENTRY
6705 A[0]=A0
6710 WRITE (Z[1],KEY=A1$)IOL=0310
6780 L0=0,V0=0
6790 GOTO 1000
6800 REM "NEXT ENTRY IN FILE
6805 ON 10*FPT(C0)+2 GOTO 6850,6806
6810 A1$=KEY(Z[1],END=6895)
6815 GOSUB 6600
6820 IF A1$="" THEN GOTO 6800 ELSE IF A1$(1,LEN(K9$))<>K9$ THEN GOTO 6895
6825 A$(1,LEN(A1$))=A1$
6830 GOSUB 6600
6835 GOSUB 6450
6840 GOTO 1515
6850 REM "PRIOR ENTRY
6855 CALL "ZZKEYP",ERR=6856,Z[1],A1$; GOTO 6820
6860 CALL "ZZKEYL",ERR=6861,Z[1],A1$; GOTO 6820
6870 GOTO 6895
6890 GOTO 1515
6895 PRINT 'RB',; READ (Z[1],KEY=K9$,DOM=1140); GOTO 1140
7500 REM "CUSTOM PROGRAMMING ROUTINES
7800 REM "GET G/L Z MASK
7810 CALL "ZZDISP","AX","000000000000","G/L",X3$,W2$,"",0,0,X4$
7815 M2=0; FOR X=1 TO LEN(W2$); M2=M2+POS("0"=W2$(X,1)); NEXT X
7820 CALL "ZZDISP","AX","000000000000","J/C",X3$,W3$,"",0,0,X4$
7825 M3=0; FOR X=1 TO LEN(W3$); M3=M3+POS("0"=W3$(X,1)); NEXT X
7830 CALL "ZZDISP","AX","000000000000","JCC",X3$,W4$,"",0,0,X4$
7835 M4=0; FOR X=1 TO LEN(W4$); M4=M4+POS("0"=W4$(X,1)); NEXT X
7840 RETURN 
7850 REM "CALC DISC AMT
7880 A[7]=A[6]*D[0]/100
7885 PRINT @(61,8+V0),A[6]:M0$,@(61,9+V0),A[7]:M0$,
7890 RETURN 
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9050 ON Y7 GOTO 9060,9100,9900,9070,9090
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9900
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9800 REM "EXIT PROGRAM
9900 REM "END PROGRAM
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9920 SETERR 9940
9930 Q1$=A1$; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
