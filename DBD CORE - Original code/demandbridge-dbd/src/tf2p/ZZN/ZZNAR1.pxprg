2040 ON X0 GOTO 2100,1800
2100 REM "Customer number
2105 L$=STR(4+L0+L1)+"AR1"
2110 CALL "ZZENTR","AXU",A{ALL},A$,X4$,X3$,19,3+L0,1,10,C0,"A/R","{3"+X$,"","ZZN00100",L$,"AR2MAB",""; IF C0=6 THEN GOSUB 6200; C0=1
2115 IF ABS(C0)=4 THEN GOTO 2140
2120 FIND (Z[1],KEY=A$(1,10),DOM=2110)IOL=0320
2125 PRINT @(34,3+L0),B$(11,30),
2140 GOTO 2020
5800 REM "Alt sort keys
