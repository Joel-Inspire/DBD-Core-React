0080 BEGIN 
0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,Q0$,Q1$
0100 SETERR 9000
0110 X0$="CAXFM4",X1$="Convert to TopForm FMS Location File"
0120 DIM Z0$(80,"-"),Z1$(20,"0")
0130 K0=20,K1=1
0135 C9=-1
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=ESC+"BG"+Z0$+ESC+"EG"
0300 REM "IOLISTS
0310 IOLIST A$,A1$,A[0],A[1],A[2],A[3],A[4],A[5],A[6],A[7],A[8],A[9],A[10],A[11],A[12],A[13],A[14],A[15]
0330 IOLIST C$,C[0],C[1],C[2],C[3],C[4],C[5],C[6],C[7],C[8],C[9],C[10],C[11],C[12],C[13],C[14],C[15]
0360 IOLIST F$,F[0],F[1],F[2],F[3],F[4],F[5],F[6],F[7],F[8],F[9],F[10],F[11],F[12],F[13],F[14],F[15],F[16],F[17],F[18],F[19],F[20],F[21],F[22],F[23],F[24],F[25],F[26],F[27],F[28],F[29],F[30],F[31]
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0507 REM ":LET J$="FM4"+X3$(9,3); CALL "CA2CMB",X3$,X4$,J$
0510 Z$="01X FM4DATA  02O FM4...  03O AR1...  06O FM1...  12O MAPDAT  13O ZZPARM  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0600 REM "
0610 GOSUB 6000
0640 CALL "ZZPROM",".Y",X3$,Z,"Proceed?","","",0; ON Z GOTO 0641,9900
0700 CALL "ZZINIT",STR(Z[2]:"00")
0710 X$=""
0715 IF X$="" THEN GOTO 0900
0720 FOR I=1 TO LEN(X$) STEP 4
0730 CALL "ZZPROM",".Y",X3$,Z,"Clear Mapping for "+X$(I,3)+"?","","",0; ON Z GOTO 0731,0790
0740 READ (Z[12],KEY=X$(I,4),DOM=0741)
0750 READ (Z[12],END=0790)R2$
0760 IF X$(I,4)<>R2$(1,4) THEN GOTO 0790
0770 REMOVE (Z[12],KEY=R2$(1,24))
0780 GOTO 0750
0790 NEXT I
0900 REM 
0910 DIM C[15],D[40],F[31]
0920 H0$=$0A$
0940 READ (Z[12],KEY="*PARAMETERS")P9$
0950 ADDR "CA2CVT"
1000 REM "Read Data file
1100 P0=POS(H0$=R1$); IF P0=0 THEN READ RECORD (Z[1],END=5000)R2$; R1$=R1$+R2$; GOTO 1100
1110 R$=R1$(1,P0-1),R1$=R1$(P0+LEN(H0$))
1120 IF FNS$(R$)="" THEN GOTO 1000
1800 REM 
1820 C1=C1+1; GOSUB 2000
1840 IF MOD(C1,10)=1 THEN PRINT @(0,7),'LD',@(0,20),C1,"/",C2,"  ",A$(1,60)
1850 REM PRINT R$; INPUT *
1900 GOTO 1000
2000 REM "Initialize TopForm Record
2100 DIM A$(120),A[15],S$(50),F9$(0)
2200 REM "Customer Code
2205 DIM X$(100); Q$=R$(1,17); X$(1)="AR1 "+Q$
2210 FIND (Z[12],DOM=2211,KEY=X$(1,24))X$; A$(1,10)=X$(25); GOTO 2220
2215 A$(1)="**Mapping entry missing** "+R$; GOTO 4990
2220 FIND (Z[3],KEY=A$(1,10),DOM=4990)IOL=0330
2225 REM "Form Number
2227 CALL "YUCASE",X3$,R$(18,10),Q$
2235 A$(11,10)=Q$
2245 EXTRACT (Z[6],KEY=A$(1,20),DOM=4990)IOL=0360
2250 REM "Location Code
2260 Q$=R$(28,4); DIM X$(100); X$(1)="FM0W"+Q$,I1=21,I2=4; GOSUB 7500
2300 REM "Counted?
2310 A$(26,1)=R$(32,1)
2330 REM "Automatic Restocking Method
2340 A$(27,1)=R$(33,1)
2350 REM "Standard Supplying Whse
2360 A$(28,4)=R$(34,4)
2370 REM "Manual Reorder Date
2380 Q$=R$(38,10)
2390 GOSUB 7600; A$(32,6)=X$
2400 REM "Last Count Date
2410 IF A$(26,1)="Y" THEN A$(75,6)="J70430"; REM "LET Q$=R$(59,10)
2420 REM "GOSUB 07600; LET A$(75,6)=X$
2430 REM "Usage Start Date
2440 Q$="070192"
2450 GOSUB 7600; A$(83,6)="J20701"
2460 REM "Last Shipment Date
2470 Q$=R$(79,10)
2480 GOSUB 7600; A$(89,6)=X$
2500 REM "Low Stock point - quantity
2510 Q$=STP(R$(114,15),0)
2520 GOSUB 7650; A[2]=X*F[0]
2530 REM "Last Count Quantity
2540 Q$=STP(R$(129,15),0)
2550 GOSUB 7650; A[3]=INT(X)*F[0]
2590 REM "Low Stock Point Days
2600 Q$=STP(R$(104,10),0)
2610 GOSUB 7650; A[1]=X
2620 REM "AVERAGE USAGE
2630 Q$=STP(R$(89,15),0)
2640 GOSUB 7650; A[0]=X*F[0]
2700 Q$=""
2710 GOSUB 7650; A[5]=-INT(X)
2760 REM "Direct Shipments since last count
2770 Q$=""
2780 GOSUB 7650; A[4]=INT(X)
2790 REM "Whse Shipments since last count
4000 REM 
4900 REM "Write file(s)
4910 WRITE (Z[2],KEY=A$(1,24))IOL=0310; C2=C2+1
4990 RETURN 
5000 REM "EOJ
5010 GOSUB 5200
5020 CALL "ZZPROM",".4",X3$,Z,"Process complete!!","","",0
5040 GOTO 9900
5200 REM "COMPLETE CYCLE
5290 RETURN 
6000 REM "BACKGROUND
6005 PRINT (0,ERR=6016)'SB',
6020 PRINT @(0,4),"This program converts the Forms Management Location file"
6165 PRINT (0,ERR=6166)'SF',
6190 RETURN 
7500 REM "Generalized Mapping
7520 FIND (Z[12],KEY=X$(1,24),DOM=7521)X$,X; A$(I1,I2)=X$(25,I2); GOTO 7590
7530 IF X$(1,4)="P/C " THEN IF LEN(FNS$(Q$))>3 THEN Q$=Q$(2); GOTO 7530 ELSE Q$=STR(NUM(Q$,ERR=7531):"000")
7560 X$(25)=Q$; WRITE (Z[12],KEY=X$(1,24))X$; GOTO 7520
7590 RETURN 
7600 REM "Generalized Date conversion
7610 IF FNS$(Q$)="" THEN X$=""; GOTO 7640
7620 CALL "CA2CVT",X3$,X4$,P9$(1,2),Q$,0,X$,0,"",0
7640 RETURN 
7650 REM "Generalized Numeric conversion
7660 IF FNS$(Q$)="" THEN X$="",X=0; GOTO 7690
7670 CALL "CA2CVT",X3$,X4$,P9$(5,2),Q$,0,X$,X,"",0
7690 RETURN 
8920 DEF FNS$(Z9$)=Z9$(1,POS("   "=Z9$+"   ")-1)
8930 DEF FNQ$(Z9$,Z9)=STR(NUM(Z9$):Z1$(1,Z9))
8940 DEF FNE$(Z9)=CHR(ASC("A")+INT(Z9/10))+STR(MOD(Z9,10))
8950 DEF FNS(Z9$)=POS(" "<>Z9$)
8960 DEF FNR$(Z9$)=Z9$(POS(" "<>Z9$))
8970 DEF FNL$(Z9)=L$(L[Z9,0],L[Z9,1])
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9055 REM 
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9900 REM "END PROGRAM
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9920 DROP "CA2CVT"
9930 SETERR 9940; Q1$=A1$; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
