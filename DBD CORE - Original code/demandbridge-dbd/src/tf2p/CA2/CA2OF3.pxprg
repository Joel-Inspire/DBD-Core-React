0702 READ (Z[12],KEY="of3I",DOM=0703)
0703 PRINT @(20,15),'CL',"Clearing mapping of initial order shipping pass (of3I)",
0704 VV$=KEY(Z[12],END=0705); IF VV$(1,4)<>"of3I" THEN GOTO 0705 ELSE REMOVE (Z[12],KEY=VV$); GOTO 0704
0705 PRINT @(20,15),'CL',
