2445 IF STP(R$(347,40))<>"" THEN GOSUB 7900
2460 ! City
2465 A$(138,16)=R$(138,16)
2480 ! State
2485 A$(154,2)=R$(154,2)
2500 ! Zip
2505 Q$=R$(156,10); GOSUB 7800; A$(156,10)=SUB(X$,"-","") ! Remove dash if found
2520 ! FOB code
2525 A$(166,1)=R$(166,1)
2540 ! How to ship code
2545 A$(167,1)=R$(167,1)
2560 ! ship via code
2565 A$(168,1)=R$(168,1)
2580 ! Inside delivery
2585 A$(169,15)=R$(169,15)
2600 ! Ship with
2605 A$(184,15)=R$(184,15)
2620 ! Mark For
2625 A$(199,35)=R$(199,35)
2640 ! Company name
2645 A$(234,35)=R$(234,35)
2660 ! Ship to Warehouse
2665 DIM X$(100); X$(1)="FM0D"+R$(269,4); X$(25)=R$(269,4); FIND (Z[12],KEY=X$(1,24),DOM=*NEXT)X$
2668 W$=X$(25,4); IF LEN(STP(W$,3))<4 THEN W=NUM(W$,ERR=*NEXT); W$=STR(W:"0000")
2670 A$(269,4)=W$
2680 ! Shipping Unit
2685 A$(273,4)=R$(273,4)
2700 ! Email Address
2705 A$(278,40)=R$(277,40)
2720 ! Ship Qty per unit
2725 Q$=R$(317,15); GOSUB 7650; A[0]=X
2740 ! Quantity to ship
2745 Q$=R$(332,15); GOSUB 7650; A[1]=X
4900 REM "Write file(s)
4910 WRITE (Z[2],KEY=A$(1,13))IOL=0310
4920 C2=C2+1
4990 RETURN 
