7925 IF STATE$<>"" AND STP(R$(154,2),3," ")="" THEN R$(154,2)=STATE$
7930 IF ZIP$<>"" AND STP(R$(156,10),3," ")="" THEN R$(156,10)=ZIP$
7945 RETURN 
