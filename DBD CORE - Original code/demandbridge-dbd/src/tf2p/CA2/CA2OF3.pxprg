2240 ! P/O Code
2245 A$(9,1)=R$(9,1)
2260 ! FOG line Number
2265 A$(10,3)=R$(10,3)
2280 ! FOG ship sequence
2285 A$(13,1)=R$(13,1)
2300 ! Location type
2305 A$(14,1)=R$(14,1)
2320 ! Ship to code
2325 DIM X$(100); X$(1)="FM0C"+R$(15,4); X$(25)=R$(15,4); FIND (Z[12],KEY=X$(1,24),DOM=*NEXT)X$
2328 W$=X$(25,4); IF LEN(STP(W$,3))<4 THEN W=NUM(W$,ERR=*NEXT); W$=STR(W:"0000")
2330 A$(15,4)=W$
2340 ! Location code
2345 A$(19,9)=R$(19,9)
2360 ! Receiving department
2365 A$(28,20)=R$(28,20)
2380 ! Attention
2385 A$(48,30)=R$(48,30)
2400 ! Address line 1
2405 A$(78,30)=R$(78,30)
2420 ! Address line 2
2425 A$(108,30)=R$(108,30)
