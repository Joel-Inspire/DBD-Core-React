0010 REM Create txt file of office supply contracts OS2TXT
0020 SETESC 9300; SETERR 9000
0035 REM "5.6 - 12/19/08 - 10.053611 - lms - SSP# 225727
0037 REM "225727-OS2TXT item code has trailing spaces can we remove          
0040 REM "Copyright 2008 Demand Bridge, LLC.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,Q0$,Q1$
0100 SETERR 9000
0110 X0$="OS2TXT",X1$="Create Text file of Office Supply Contract Prices",K9$="",K9=0
0120 DIM Z0$(80,"-")
0121 DIM B$(100)
0130 K0=20,K1=1
0135 C9=-1
0140 Z1$="#######.00-"
0170 IF POS("MS"=UCS(SYS)) THEN ISWIN=1 ELSE ISWIN=0
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=MNM('GS')+Z0$+MNM('GE')
0300 REM "IOLISTS
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01O OS1...  02O OS0...  07O OS7...  08OSOS1...  09O OS5...  13O ZZPARM  "; REM SSP 222113
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0521 SEQ=1
0522 SEQ$=STR(SEQ:"000")
0590 REM FILENAME$="OS1SCAN_"+SEQ$
0595 REM GOSUB 7700
0600 REM "
0610 GOSUB 6000
0611 DIM PLAN$(6); CALL "ZZENTR","SZXUX",A{ALL},PLAN$,X4$,X3$,35,14,1,6,C0,"","{3"+X$,"","OS2MAA04","","",""
0612 IF C0=-4 THEN GOTO 9900
0613 IF PLAN$(1,3)<>"ALL" THEN READ (Z[9],KEY=PLAN$,DOM=0611)OS5$
0615 DIM COMPARE$(6); CALL "ZZENTR","SZXUX",A{ALL},COMPARE$,X4$,X3$,35,16,1,6,C0,"","{3"+X$,"","OS2MAA04","","",""
0616 IF C0=-4 THEN GOTO 9900
0617 IF COMPARE$<>DIM(6) THEN READ (Z[9],KEY=COMPARE$,DOM=0615)
0620 DIM DOWN$(1); CALL "ZZENTR","Y",A{ALL},DOWN$,X4$,X3$,35,18,1,1,C0,"","{3"+X$,"","OS2MAA04","","",""
0621 DIM ITEM_DOWN$(1); CALL "ZZENTR","Y",A{ALL},ITEM_DOWN$,X4$,X3$,35,19,1,1,C0,"","{3"+X$,"","OS2MAA04","","",""
0640 IF Q1$="" THEN CALL "ZZPROM",".Y",X3$,Z,"PROCEED?","","",0; ON Z GOTO 0641,9900
0700 IF PLAN$=DIM(6) OR STP(PLAN$,1)="ALL" THEN FILENAME$="OS1SCAN_" ELSE FILENAME$="OS1SCAN_"+STP(PLAN$,1)+"_"
0710 GOSUB 7700
1000 REM 
1010 K$=KEY(Z[1],END=9900); READ (Z[1])
1011 PRINT @(35,20),K$(1,10)
1015 IF PLAN$(1,3)<>"ALL" AND PLAN$<>DIM(6) AND PLAN$(1,6)<>K$(11,6) THEN GOTO 1010; REM ONLY PRINT ONE PLAN
1020 READ RECORD (Z[1],KEY=K$)A$
1021 READ RECORD (Z[9],KEY=A$(11,6),DOM=*NEXT)OS5_1$
1022 IF DOWN$(1,1)="Y" THEN GOTO 1026; REM SSP 217383 - include all plans including those downloaded
1023 IF OS5_1$(1,6)<>A$(11,6) THEN GOTO 1025; REM IF NO PLAN CAN'T FIGURE OUT WHAT TYPE
1025 IF MID(OS5_1$,39,1)<>DIM(1) THEN GOTO 1000
1026 GOSUB 1200
1030 CALL "ZZDISP","DX",A$(25,6),"   ",X3$,X$,"",0,0,X4$; START_DT$=X$; REM FORMAT DATE
1032 CALL "ZZDISP","DX",A$(31,6),"   ",X3$,X$,"",0,0,X4$; END_DTE$=X$; REM FORMAT DATE
1040 READ (Z[2],KEY=A$(1,10),DOM=1000)OS0$
1041 DIM B$(57," ")
1042 READ (Z[8],KEY=A$(1,10)+COMPARE$,DOM=*NEXT)B$
1043 IF ITEM_DOWN$(1,1)="Y" THEN ITEM_DESC$=OS0$(37,40) ELSE ITEM_DESC$=DIM(40," ")
1050 PRINT (32502)A$(11,6),",",STP(OS0$(548,20),1),",",A$(1,10),",",START_DT$,",",END_DTE$,",",STR(NUM(A$(47,10)):Z1$),",",STR(COST:Z1$),",",STR(NUM(B$(47,10)):Z1$),",",STP(ITEM_DESC$,3,","); REM SSP 225727
1055 COUNT=COUNT+1
1056 IF COUNT=65000 THEN CLOSE (32502); GOSUB 2000
1100 GOTO 1000
1200 REM GET LOWEST COST
1201 COST=0,COST$=""
1202 B$(16,6)=X3$(21,6)
1225 READ (Z[7],KEY=A$(1,10),DOM=*NEXT)
1230 OS7_KEY$=KEY(Z[7],END=1299)
1232 IF OS7_KEY$(1,10)<>A$(1,10) THEN GOTO 1299
1233 READ (Z[7],KEY=OS7_KEY$)OS7$
1234 IF B$(16,6)<OS7$(77,6) OR B$(16,6)>OS7$(83,6) THEN GOTO 1230
1242 COST$=OS7$(27,10)
1250 IF COST=0 THEN COST=NUM(COST$)
1255 IF NUM(COST$)<COST THEN COST=NUM(COST$)
1264 GOTO 1230
1299 RETURN 
2000 REM OPEN SECOND FILE
2001 SEQ=SEQ+1
2002 GOSUB 7700
2003 COUNT=0
2099 RETURN 
5000 REM "EOJ
5020 REM CALL "ZZPROM",".4",X3$,Z,"Process complete!!","","",0
5040 GOTO 9900
6000 REM "BACKGROUND
6001 REM IF %GUI<>0 THEN GOTO 6190
6005 PRINT (0,ERR=6016)'SB',
6020 PRINT @(0,3),"This utility program will create comma delimited files containing Office Supply"
6021 PRINT @(0,4),"prices in a file or series of files called OS1SCAN_ +001 ,etc. "
6022 PRINT @(0,6),"If you enter a single price plan, the file will be OS1SCAN_+ Plan name_  +001."
6023 PRINT @(0,8),"There will be a maximum of 65000 records in each file"
6024 PRINT @(0,10),"You can also enter a Comparison Price Plan. If there is a price available"
6025 PRINT @(0,11),"for an item under this plan, it will be exported into your file."
6040 PRINT @(10,14),"Enter Price Plan or ALL:",
6050 PRINT @(12,16),"Comparison Price Plan:",; REM SSP 222113
6051 PRINT @(3,18),"Include downloaded Price Plans?",; REM SSP 222113
6062 PRINT @(9,19),"Include Item Description?",; REM SSP 222113/222958
6165 PRINT (0,ERR=6166)'SF',
6190 RETURN 
7700 REM "Create file, open it, lock it, write record, close file                                                                              
7701 ERASE FILENAME$+SEQ$,ERR=*NEXT
7710 SERIAL FILENAME$+SEQ$,7,0,ERR=*NEXT
7715 OPEN LOCK (32502,OPT="TEXT")FILENAME$+SEQ$; LOCK (32502)
7740 RETURN 
