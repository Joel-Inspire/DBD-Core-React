0010 REM "Get telemarketing stuff <ZP2UT8> SIGFORMS
0100 BEGIN 
0120 DIM T[20]
0410 OPEN (1)"SIGF0993"
0890 X9$=$0D0A$
1020 P=POS(X9$=A$); IF P=0 THEN READ RECORD (1)X$; A$=A$+X$; GOTO 1020
1030 R0$=A$(1,P-1),A$=A$(P+LEN(X9$))
1200 REM 
1220 P=POS(","=R0$),Q$=R0$(P+1)
1225 P=POS(","=Q$),Q$=Q$(1,P-1)
1310 GOTO 1340
1320 PRINT R0$(1,70)
1325 PRINT Q$
1340 P=POS(Q$=R9$,3); IF P=0 THEN R9$=R9$+Q$; GOTO 1340
1345 P=INT(P/3),T[P]=T[P]+1
1390 GOTO 1000
7020 FOR X=1 TO LEN(R9$) STEP 3
7025 PRINT R9$(X,3),"  ",T[INT(X/3)]
7040 NEXT X
