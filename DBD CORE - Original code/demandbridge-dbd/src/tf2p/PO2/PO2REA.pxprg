2510 DIM P$(Y8[0]*W3); I0=(Y8[2]-1)*W3,L0=1
2520 IF POS(" "<>B$(179,10))=0 THEN GOTO 2550
2530 CALL "ZZDISP","AX",B$(179,10),"A/R",X3$,F5$,"",0,0,X4$
2540 F5=1; GOSUB 8300
2550 F5=2,F5$=FNY$(FND$(A$(39,6))); GOSUB 8300
2560 F5=3,F5$=C$(11,35); GOSUB 8300
2570 F5=4,F5$=B$(328,15); GOSUB 8300
2580 F5=5,F5$=C$(165,20); GOSUB 8300
2585 F5=19,F5$=F$(16,30); GOSUB 8300
2590 F5=20,F5$=F$(12,4); GOSUB 8300
2890 RETURN 
