1490 READ (Z[5],KEY=A$(1,9)+A$(20,3),DOM=1491)IOL=0350
1500 REM "Print Line Items
1520 K1$=KEY(Z[5],END=1000); READ (Z[5],KEY=K1$)IOL=0350
1530 IF A$(1,9)+A$(20,3)<>K1$(1,12) THEN GOTO 1000
1535 IF F1$="Y" AND A$(1,9)<>L9$ THEN GOSUB 3000
1540 IF F1$="Y" THEN GOSUB 2500; F1$="P"
1550 IF L0>Y8[3] THEN GOSUB 2900; GOSUB 2500
1560 F5=7,F5$=D$(101,10); GOSUB 8350
1580 F5=8,F5$=D$(28,30); GOSUB 8350
1600 F5=9,F5$=E$(100,10); GOSUB 8350
1620 F5=10,F5$=STR(D[2]:M1$); GOSUB 8350
1640 GOSUB 7500; F5=11,F5$=X$; GOSUB 8350
1700 F5=12,F5$=STR(D[1]:M2$); GOSUB 8350
1740 F5=13,F5$=STR(E[4]:M3$); GOSUB 8350
1780 F5=14,F5$=STR(E[3]:M1$); GOSUB 8350
1810 IF F9=1 THEN X=12345678; GOTO 1830
1820 X=E[4]*E[3]; IF E[2]<>0 THEN X=X*E[2]
1830 F5=15,F5$=STR(X:M4$); GOSUB 8350
1840 CALL "ZZDISP","AX",A$(1,9),"P/O",X3$,F5$,"",0,0,X4$
1850 F5=16,F5$=F5$(4); GOSUB 8350
2400 REM 
2420 L0=L0+1,I0=I0+F4*W3
