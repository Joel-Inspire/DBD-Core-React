0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,Q0$,Q1$
0100 SETERR 9000
0110 X0$="ZR2UT3",X1$="Build Archive Files for JetForm"
0120 DIM Z0$(80,"-")
0130 K0=20,K1=1
0135 C9=-1
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=MNM('GS')+Z0$+MNM('GE')
0300 REM "IOLISTS
0310 IOLIST A$,A[0],A[1],A[2],A[3],A[4],A[5],A[6],A[7],A[8],A[9],A[10],A[11],A[12],A[13],A[14],A[15],A[16],A[17],A[18],A[19],A[20],A[21],A[22],A[23],A[24],A[25],A[26],A[27],A[28],A[29],A[30]
0330 IOLIST C1$,C$,C[0],C[1],C[2],C[3],C[4],C[5],C[6],C[7]
0350 IOLIST E1$,E$,E[0],E[1],E[2],E[3],E[4],E[5],E[6],E[7],E[8],E[9],E[10],E[11],E[12],E[13],E[14],E[15],E[16],E[17],E[18],E[19],E[20]
0380 IOLIST C$,C[0],C[1],C[2],C[3],C[4],C[5],C[6],C[7]
0400 IOLIST E$,E[0],E[1],E[2],E[3],E[4],E[5],E[6],E[7],E[8],E[9],E[10]
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01O ARB...  02O ARC...  03O ARD...  04O ASE...  05O ART...  06O ZRA...  07O ZRB...  08O ZRC...  09O ZRD... 10O ZRE...  13O ZZPARM  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0600 REM "
0610 REM "GOSUB 06000
0620 REM "GOSUB 08100
0640 REM "IF Q1$="" THEN CALL "ZZPROM",".Y",X3$,Z,"PROCEED?","","",0; ON Z GOTO 00641,09900
0800 DIM A$(689),A[30],E$(289),E[20],C$(37),C[7]
0900 READ (Z[1],KEY="",DOM=0901)
1000 REM 
1020 READ (Z[1],END=2100)IOL=0310
1030 JL=A[20],JL1=A[0],C=0
1040 WRITE (Z[6],KEY=A$(1,14))IOL=0310
1050 GOSUB 2000
1600 GOTO 1000
2000 REM "Process ARB and ART, ARD files
2010 READ (Z[5],IND=JL)IOL=0350
2020 JL=NUM(E1$)
2030 C=C+1,E$=A$(1,14)+STR(C:"000")+E$(3)
2035 WRITE (Z[10],KEY=A$(1,14)+STR(C:"000"))IOL=0400
2040 IF JL=-1 THEN GOTO 2050
2045 GOTO 2010
2050 C=0; IF JL1=0 THEN GOTO 2090
2052 READ (Z[3],IND=JL1)IOL=0330
2055 JL1=NUM(C1$),C=C+1,C$=A$(1,14)+STR(C:"000")+C$(1,37)
2065 WRITE (Z[8],KEY=A$(1,14)+STR(C:"000"))IOL=0380
2070 IF JL1=-1 THEN GOTO 2090
2075 GOTO 2050
2090 RETURN 
2100 REM "Process ARC file to ZRB file
2110 READ RECORD (Z[2],END=2200)TMP$
2150 WRITE RECORD (Z[7],KEY=TMP$(1,14))TMP$
2190 GOTO 2110
2200 REM "Process ASE file to ZRD file
2205 K$=KEY(Z[4],END=2300)
2210 READ RECORD (Z[4],KEY=K$)ASE$
2250 WRITE RECORD (Z[9],KEY=K$)ASE$
2290 GOTO 2205
5000 REM "EOJ
5040 GOTO 9900
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9055 REM 
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9900 REM "END PROGRAM
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9930 SETERR 9940; Q1$=A1$; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
