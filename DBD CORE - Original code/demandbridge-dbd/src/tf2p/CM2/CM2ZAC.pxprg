1020 ENTER X3$,F0,B$,B{ALL}
7500 REM "Q1 IS PRO-RATION FACTOR FOR SALES TAX
7510 Q1=100; IF B[0]<>0 THEN Q1=(B[6]+B[7]+B[8])*100/B[0]
7512 IF B[6]=0 THEN Q1=0
7515 B[27]=Q1
7600 REM "CALCULATE TOTALS
7605 B[28]=B[6]-B[22]-(B[4]-B[17])-(Q1*B[2]/100-B[18])
7606 B[29]=0,B[30]=0
7610 IF B[0]>0 AND B[6]+B[7]+B[8]=0 THEN GOTO 7650
7619 REM "Get rates, will get rate from first record that matches source company/job number part of key. Generally the 000 record but the 001 record for cost only jobs
7620 J$=B$(1,3)+B$(9,9); CALL "CM2ZAD",X3$,J$,X{ALL}; REM CALL "CM2ZAB",X3$,F0,B$(1,3),X(ALL)
7625 B[29]=B[28]*(X[0]+X[1])/100
7635 B[30]=B[6]-B[22]-(B[3]-B[16])-B[29]
7639 REM "Only add reclaimed costs into net proceed when invoice balance =0
7640 REM IF B(5)-B(12)<>0 THEN IF B(0)=B(6)+B(7)+B(8) THEN LET B(30)=B(30)+B(5)-B(12)
7641 REM "I think obsoleted 9/28/94
7645 REM IF B(29)<>0 THEN IF B(0)=B(6)+B(7)+B(8) THEN IF B(12)<>0 THEN LET B(30)=B(30)+B(3)
7680 GOTO 9900
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5); IF Y5=68 OR Y5=69 THEN GOTO 9500
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9055 REM 
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9120 ON Y8 GOTO 9900,0990,9920
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)<>"1" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; SETESC 9300; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9740 RETURN 
9750 REM "FILES
9760 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1
9790 RETURN 
9800 REM "
9900 REM "END PROGRAM
9950 EXIT 
9999 END 
