0010 REM "Re-process parameter records <CM2UT2>
0100 BEGIN 
0410 OPEN (1)"ZZPARM"
1020 K$=KEY(1,END=5000); READ RECORD (1)A$
1040 IF POS("%%%"=K$)=0 THEN IF POS("%%%"=A$)>0 THEN GOTO 1200
1090 GOTO 1000
1200 REM "
1202 ESCAPE 
1220 P=POS("%%%"=A$)
1225 A$(P,3)=K$(P,3)
1250 WRITE RECORD (1,KEY=K$)A$
1285 R1=R1+1
1290 PRINT ".",
1295 GOTO 1000
