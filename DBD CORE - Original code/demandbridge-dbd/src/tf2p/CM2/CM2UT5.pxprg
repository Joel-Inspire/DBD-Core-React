0010 REM "MERGE CORRECT PRODUCT CODES INTO FM1S
0100 BEGIN 
0410 OPEN (1)"FM1PC"
0420 OPEN (2)"ARH144"
0990 READ (1,KEY="02",DOM=0991)
1000 K$=KEY(1,END=5000); READ (1)A$
1020 READ (2,KEY=K$(1,2))B$
1025 R0=R0+1
1026 IF MOD(R0,250)=0 THEN PRINT R0," RECORDS","  ",R1
1040 CLOSE (3); OPEN (3)"FM1"+B$(33,3)
1080 EXTRACT RECORD (3,KEY=K$,DOM=1210)Q$
1085 Q$(21,3)=A$
1090 WRITE RECORD (3,KEY=K$)Q$
1095 R1=R1+1
1200 REM 
1210 CLOSE (3)
1290 GOTO 1000
