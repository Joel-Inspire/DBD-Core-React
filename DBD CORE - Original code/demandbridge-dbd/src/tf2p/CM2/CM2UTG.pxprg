0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0550 IF X3$(330,1)<>"1" THEN CALL "ZZPROM",".4",X3$,0,"This utility may only be used by Remote Locations of Proforma.","","",0; GOTO 9900
0600 REM "
0610 GOSUB 6000
