1110 U9$=U$
1120 ON V0 GOTO 1150,1150,1130,1140
1150 READ (Z[1],KEY=U$,DOM=1000)IOL=0310
1400 REM "Subtotals"
1410 GOSUB 3100
1420 ON T GOTO 1470,1430,1440,1450,1460
1430 GOSUB 6200
1440 GOSUB 6300
1450 GOSUB 6400
1490 T=0
1500 REM "Print Data
1503 IF POS(" "<>A$(7,3))=0 THEN GOTO 1000; REM "If header record, don't print and skip counting the record @ 1900s
1505 IF W+2>W0 THEN GOSUB 6000
1510 DIM C$(100); FIND (Z[3],KEY=A$(7,3)+"   ",DOM=1511)IOL=0330
1515 C0=0,C0=NUM(A$(10,5),ERR=1516)
