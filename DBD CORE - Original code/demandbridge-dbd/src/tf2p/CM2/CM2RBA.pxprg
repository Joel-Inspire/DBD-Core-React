6315 IF A$(15,1)="Y" THEN Y5$(32)="OK" ELSE Y5$(32)="NOT OK"
6320 CALL "ZZDISP","DXS",A$(21,6),"",X3$,Y5$,"",39,0,X4$
6325 CALL "ZZDISP","TXS",A$(27,4),"",X3$,Y5$,"",50,0,X4$
6330 Y5$(60)="Operator: ",Y5$(70)=A$(31,3)
