0010 REM "Check for Ship-to on Open Order <FS2DAL>
0020 SETESC 9300; SETERR 9000
0035 REM "4.1 - 02/17/97 - 16.63 - tma - SSP# 081634
0040 REM "Copyright 1997 Computer Software Inc.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,ITEM$,SHIPTO$,CUST$
0100 SETERR 9000
0110 X0$="FS2DAL",Z=99
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01O FS5...  02O PO3...  03O FS1...  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
1000 REM "
1020 READ (Z[1],KEY=ITEM$,DOM=1021)
1040 A$=KEY(Z[1],END=9900); READ (Z[1])
1060 IF A$(1,LEN(ITEM$))<>ITEM$ THEN GOTO 9900
1070 READ (Z[3],KEY=A$(25,8),DOM=1040)C$
1075 IF C$(6,10)<>CUST$ THEN GOTO 1040
1080 READ (Z[2],KEY=A$(25,8),DOM=1081)
1100 READ (Z[2],END=1800)B$
1120 IF A$(25,8)<>B$(166,8) THEN GOTO 1800
1140 IF B$(11,1)="C" AND SHIPTO$=B$(12,4) THEN IF B$(7,3)="   " OR B$(7,3)=A$(33,3) THEN ORDER$=A$(25,8); GOTO 2000
1160 GOTO 1100
1800 REM 
1990 GOTO 1040
2000 REM 
2020 CALL "ZZPROM",".4",X3$,0,"CAUTION: Order "+ORDER$+" also has ship-to code "+SHIPTO$+" on it.","","",0
2040 GOTO 9900
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9900 REM "END PROGRAM
9905 PRINT @(0,22),'CL',
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9930 SETERR 9940; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
