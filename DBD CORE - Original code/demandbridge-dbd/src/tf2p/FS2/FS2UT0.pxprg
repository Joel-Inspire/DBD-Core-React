5040 GOTO 9900
6000 REM "BACKGROUND
6005 PRINT (0,ERR=6016)'SB',
6040 PRINT @(0,4),"This utility processes the Bundled Pricing override data",'LF',"which may have been setup on systems with a Topform release level",'LF',"prior to 3.7.  The old bundled pricing file is left intact as AS7ccc36,",'LF',"where ccc is your company code.",
6165 PRINT (0,ERR=6166)'SF',
6190 RETURN 
7500 REM "Re-do files
7505 CLOSE (14); F9=-1; OPEN (14,ERR=7550)"AS7"+X3$(9,3); F9=1
7510 CLOSE (14); OPEN (14,ERR=7515)"AS7"+X3$(9,3)+"36"; CALL "ZZPROM",".4",X3$,0,"The file AS7ccc36 already exists.","","",0; GOTO 9900
