1230 A$(1,10)=B$(1,10),A$(11,10)=B$(12,10),A$(21,9)=B$(22,8)
1240 A$(36,4)=B$(101,4),A$(76,4)=A$(36,4)
1245 A[0]=B[2],A[17]=A[0],A[1]=A[0],A[2]=A[1]
1260 A[8]=B[1],A[9]=A[8],A[18]=B[1],A[19]=100
1400 REM 
