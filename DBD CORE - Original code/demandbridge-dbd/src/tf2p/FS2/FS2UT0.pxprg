7525 CALL "ZZPROM",".4",X3$,0,"Cannot open old bundled pricing file.","","",0; GOTO 9900
7530 LOCK (14,ERR=7535); Q$=FN%FID$(14); D=ASC(Q$(20,1)); GOTO 7540 ! [205703]-changed FID() to FN%FID$()
