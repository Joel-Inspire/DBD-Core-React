7540 CLOSE (14); RENAME "AS7"+X3$(9,3),"AS7"+X3$(9,3)+"36"
7550 DIRECT "AS7"+X3$(9,3),29,5000,256,D,0
7555 IF F9=-1 THEN CALL "ZZPROM",".4",X3$,0,"The bundled pricing file has been defined.","","",0; GOTO 9900
7590 RETURN 
