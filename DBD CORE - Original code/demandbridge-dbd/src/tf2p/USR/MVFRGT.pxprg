2010 X$="097010001020100010301000100010001050100010101000"
2020 REM "READ (2,KEY="09701000",DOM=02021)
2030 K$=KEY(2,END=5000)
2035 REM "IF K$(1,10)<>"09701000" THEN ESCAPE
2040 READ (2,KEY=K$)IOL=0320
