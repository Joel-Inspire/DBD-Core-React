0100 BEGIN 
0200 OPEN (1)"IC0323"
0300 K1$=KEY(1)
0400 READ RECORD (1,KEY=K1$)A$
0500 IF POS(" "<>A$(1,10))=0 THEN GOTO 0300
0600 J=NUM(A$(1,10))
0700 J$="00"+STR(J:"00000")+"000"
0800 A$(1,10)=J$
0900 WRITE RECORD (1,KEY=A$(1,LEN(K1$)))A$
0950 REMOVE (1,KEY=K1$)
1000 PRINT A$(1,LEN(K1$))
1100 GOTO 0300
