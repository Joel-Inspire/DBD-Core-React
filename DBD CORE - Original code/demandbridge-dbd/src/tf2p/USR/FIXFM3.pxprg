0010 REM "Remove bad FM3 records and write good ones back out
0210 OPEN (1)"/usr/lib/pvx/z.470/tf2d/FM3470"
0220 OPEN (2)"FM3JEL"
0900 REM "CUSTOMERS TO FIND : 0004105, 0001100
1000 REM "READ THRU FILE TO FIND BAD RECORDS
1010 READ (1,KEY="0004105",DOM=1011)
1015 READ (2,KEY="0004105",DOM=1016)
1020 READ RECORD (1,END=5000)A$
1030 IF A$(1,7)<>"0004105" THEN GOTO 5000
1050 READ RECORD (2,END=1020)B$
1060 IF B$(1,29)<>A$(1,29) THEN GOTO 1050
1065 REMOVE (1,KEY=A$(1,29))
1070 WRITE RECORD (1,KEY=B$(1,29))B$
1080 GOTO 1020
