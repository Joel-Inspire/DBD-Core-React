0100 OPEN (1)"XXPARM"
0200 OPEN (2)"ZZPARM"
0205 READ (1,KEY="AA",DOM=0206)
0300 K$=KEY(1); READ (1)
0303 IF K$(1,1)>"I" THEN ESCAPE 
0305 PRINT K$,
0400 IF K$(1,2)="GR" OR K$(1,4)="APPS" OR POS("INSTALL"=K$)<>0 THEN PRINT "*"; GOTO 0401 ELSE PRINT ""; GOTO 0300
0500 READ RECORD (1,KEY=K$)A$
0600 WRITE RECORD (2,KEY=K$)A$
0700 GOTO 0300
