0010 REM "Build EM1 file from AR1 file
0110 DIM A$(600),A[15],B$(120)
0210 OPEN (1)"AR1288"
0220 OPEN (2)"EM1288"
0310 IOLIST A$,A[0],A[1],A[2],A[3],A[4],A[5],A[6],A[7],A[8],A[9],A[10],A[11],A[12],A[13],A[14],A[15]
0320 IOLIST B$
2000 REM "READ FROM AR1 FILE
2010 K$=KEY(1,END=5000)
2020 READ (1,KEY=K$)IOL=0310
2050 IF A$(1,3)<>"02B" OR A$(1,3)<>"02D" THEN GOTO 2010
3000 REM "WRITE OUT TO EM1 FILE
3010 B$(1,15)=A$(1,10),B$(16,35)=A$(11,35),B$(51,10)=A$(1,10),B$(71,1)="Y",B$(72,1)="N",B$(73,1)="N",B$(74,1)="Y"
3020 IF A$(1,3)="02D" THEN B$(75,45)="/usr/lib/pvx/denny" ELSE B$(75,45)="/usr/lib/pvx/black"
3050 WRITE (2,KEY=B$(1,15))IOL=0320
3090 GOTO 2010
5000 REM "eoj
5005 ESCAPE 
5010 GOTO 9900
