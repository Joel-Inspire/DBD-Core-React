1535 FOR X=0 TO 14
1536 SETERR 1560
1540 IF A2$(LEN(A2$),1)<>"N" THEN A[X+15]=DEC(X$(A1+X*5,5))/100 ELSE A[X+15]=DEC(X$(A1+X*5,5))
1560 SETERR 9000
1570 NEXT X
