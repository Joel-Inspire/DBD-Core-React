0010 REM "Process VERTEX file and update Topform <CA2AR5>
0020 SETESC 9300; SETERR 9000
0035 REM "4.1 - 04/23/98 - 10.69 - dmm - SSP# 091564
0040 REM "Copyright 1997 Computer Software Inc.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0080 BEGIN 
0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,Q0$,Q1$
0100 SETERR 9000
0110 X0$="VT2LTT",X1$="Convert VERTEX Records"
0120 DIM Z0$(80,"-"),Z1$(20,"0"),C[15],D[4],E[4]
0130 K0=20,K1=1
0135 C9=-1
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=MNM('GS')+Z0$+MNM('GE')
0300 REM "IOLISTS
0310 IOLIST A$,A[0],A[1],A[2],A[3]
0330 IOLIST C$,C[0],C[1],C[2],C[3],C[4],C[5],C[6],C[7],C[8],C[9],C[10],C[11],C[12],C[13],C[14],C[15]
0340 IOLIST D$,D[0],D[1],D[2],D[3]
0350 IOLIST E$,E[0],E[1],E[2],E[3]
0360 IOLIST F$
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01X tapefile 02O AR5...  03O AR1... 04O ASM... 05O FM0... 13O ZZPARM  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0530 READ (Z[13],KEY=X3$(9,3)+"A/R")P0$
0540 CLOSE (Z[1]); OPEN (Z[1],ISZ=200)"tapefile"
0600 REM "
0605 REM "ESCAPE
0606 GOTO 6300
0610 GOSUB 6000
0620 GOSUB 8200
0630 GOSUB 8100
0635 CLOSE (Z[1]); OPEN LOCK (Z[1],OPT="TEXT")"tapefile"
0640 CALL "ZZPROM",".Y",X3$,Z,"Proceed?","","",0; ON Z GOTO 0641,9900
0830 CALL "ZZINIT",STR(Z[4]:"00")
0850 GOSUB 6200; REM "BUILD ZIP FILE
0860 REM "ESCAPE
0870 REM "GOTO 06300
0900 REM 
0910 REM "ESCAPE
0920 H0$=$0A$
0950 CLOSE (Z[1]); OPEN LOCK (Z[1],OPT="TEXT")"tapefile"
1000 REM "Read Data file
1100 READ (Z[1],END=5000)R$
1110 IF R$(1,2)="99" THEN GOSUB 8250; GOTO 5000
1170 C1=C1+1
1180 GOSUB 2000
1300 IF MOD(C1,T0)=1 THEN GOSUB 8150; PRINT @(0,14),'LD',@(15,20),A$(1,2)+"-"+A$(3,4)+"-"+A$(7,4)+"  "+A$(11,25),
1900 GOTO 1000
2000 REM "Initialize TopForm Record
2100 DIM A$(119),A[3]; REM "WO91564, DIM was 55 changed to 119
2120 IF R$(2,2)="00" THEN GOTO 4915
2130 IF NUM(R$(11,1))>8 THEN GOTO 4915
2140 A$(1,10)=R$(2,2)+"0"+R$(4,7)
2145 IF R$(11,1)="1" THEN A$(3)=""
2150 IF R$(11,1)="2" THEN A$(7)=""
2159 REM "Set tax freight flag here, if record exists it will override this default setting.
2160 IF R$(99,1)="T" THEN A$(36,1)="Y" ELSE A$(36,1)="N"
2164 REM "See if record already exists, if so read in and update info in it.
2165 READ (Z[2],KEY=A$(1,10),DOM=2166)IOL=0310
2175 A$(11,25)=R$(25,20)
2177 PRECISION 3
2180 IF R$(11,1)="1" THEN A[0]=NUM(R$(45,5))/1000 ELSE IF R$(11,1)="2" THEN A[0]=NUM(R$(45,5))/1000 ELSE IF R$(11,1)="3" THEN A[0]=NUM(R$(50,5))/1000 ELSE A[0]=NUM(R$(50,5))/1000
2200 REM "Update control and date fields
2210 A$(37,6)=X3$(21,6)
2215 A$(43,6)=H1$
2220 A$(49,6)=FNE$(NUM(R$(16,2),ERR=2221))+R$(18,4)
2300 REM " FIND OTHER SALES TAX
2310 IF R$(11,1)="3" OR R$(11,1)="4" THEN DIM D$(54),D[4]; FIND (Z[2],KEY=A$(1,6)+"    ",DOM=2311)IOL=0340
2315 IF A$(1,6)<>D$(1,6) THEN GOTO 2320
2317 A[2]=D[0]
2320 DIM E$(54),E[4]; FIND (Z[2],KEY=A$(1,2)+"        ",DOM=2321)IOL=0350
2330 IF A$(1,2)<>D$(1,2) THEN GOTO 2400
2390 A[3]=E[0]
4900 REM "Write file(s)
4905 IF POS(" "<>A$(11))=0 THEN GOTO 4915
4910 WRITE (Z[2],KEY=A$(1,10))IOL=0310; C2=C2+1
4911 PRECISION 2
4912 JL$=A$(1,10)
4915 RETURN 
5000 REM "EOJ
5010 IF T9$<>"Y" THEN X$=STR(C1)+" records done. Did not find a valid trailer record, data is incomplete!!!"; CALL "ZZPROM",".4",X3$,Z,X$,"","",0; GOTO 5021
5015 X$="Process complete - "+STR(C2)+" of "+STR(C1)+" records loaded."
5020 CALL "ZZPROM",".4",X3$,Z,X$,"","",0
5040 GOTO 9900
5200 REM "COMPLETE CYCLE
5290 RETURN 
6000 REM "BACKGROUND
6005 PRINT (0,ERR=6016)'SB',
6020 PRINT @(0,4),"This program loads the Vertex Tax Data from a file named 'tapefile' into ",'LF',"the TopForm Sales Tax File (AR5)",
6165 PRINT (0,ERR=6166)'SF',
6190 RETURN 
6200 REM "BUILD ZIP FILE
6210 READ (Z[1],END=6290)A$
6215 IF A$(11,1)="1" THEN STATE$=A$(97,2)
6220 IF A$(11,1)="0" THEN GOTO 6210
6225 IF A$(11,1)="3" OR A$(11,1)="8" THEN CITY$=A$(25,25)
6230 ZIP$=A$(111,5)+A$(120,5),GEOCODE$=A$(2,2)+"0"+A$(4,7)
6235 IF A$(11,1)="1" THEN ZIP$=A$(109,10)
6237 XT$=ZIP$+GEOCODE$+STATE$+CITY$
6238 IF ZIP$="0000000000" THEN GOTO 6285
6240 PRINT @(10,18),GEOCODE$
6270 WRITE (Z[4],KEY=ZIP$+GEOCODE$)XT$
6280 ZIP$="",GEOCODE$="",CITY$=""
6285 GOTO 6210
6290 RETURN 
6300 REM "REBUILD AR1 AND FM0 FILE W/ CORRECT SALES TAX AND STATE CODE
6301 ESCAPE 
6305 PRINT @(0,14),'CE'
6310 K$=KEY(Z[3],END=6500)
6315 IF K$(1,10)="0000002085" THEN ESCAPE 
6320 READ (Z[3],KEY=K$)IOL=0330
6330 IF STP(C$(1,10),3," ")="" THEN GOTO 6310
6350 READ (Z[4],KEY=C$(134,5),DOM=6351)
6360 J$=KEY(Z[4],END=6310)
6370 READ (Z[4],KEY=J$)
6375 IF STP(C$(134,5),3," ")="" THEN GOTO 6310
6380 IF NUM(C$(134,5))>=NUM(J$(1,5)) AND NUM(C$(134,5))<=NUM(J$(6,5)) THEN C$(235,10)=J$(11,10); GOTO 6395
6390 PKEY$=KEP(Z[4]); IF NUM(C$(134,5))>=NUM(PKEY$(1,5)) AND NUM(C$(134,5))<=NUM(PKEY$(6,5)) THEN C$(235,10)=PKEY$(11,10) ELSE GOTO 6700
6395 PRINT @(10,18),K$
6400 WRITE (Z[3],KEY=K$)IOL=0330
6410 GOTO 6310
6500 REM "FIX FM0 FILE
6505 ESCAPE 
6510 J$=KEY(Z[5],END=5000)
6520 READ (Z[5],KEY=J$)IOL=0360
6525 READ (Z[4],KEY=F$(129,5),DOM=6526)
6530 X$=KEY(Z[4])
6540 READ (Z[4],KEY=X$)
6545 IF STP(F$(129,5),3," ")="" THEN GOTO 6510
6550 IF NUM(F$(129,5))>=NUM(X$(1,5)) AND NUM(F$(129,5))<=NUM(X$(6,5)) THEN F$(224,10)=X$(11,10); GOTO 6600
6560 PKEY$=KEP(Z[4]); IF F$(129,5)>=PKEY$(1,5) AND F$(129,5)<=PKEY$(6,5) THEN F$(224,10)=PKEY$(11,10) ELSE GOTO 6800
6590 PRINT @(10,18),J$
6600 WRITE (Z[5],KEY=J$)IOL=0360
6610 GOTO 6510
6700 REM "CK FOR ZIP CODE IN ANY PART OF FILE
6710 READ (Z[4],KEY=C$(134,3),DOM=6711)
6720 JL$=KEY(Z[4],END=6400)
6730 READ (Z[4],KEY=JL$)
6740 IF C$(134,5)>JL$(1,5) THEN GOTO 6410
6750 IF C$(134,5)>=JL$(1,5) AND C$(134,5)<=JL$(6,5) THEN C$(235,10)=JL$(11,10); GOTO 6395
6760 GOTO 6720
6800 REM "CHK ZIP CODE FILE
6810 READ (Z[4],KEY=F$(129,4),DOM=6811)
6820 JL$=KEY(Z[4],END=6610)
6830 READ (Z[4],KEY=JL$)
6840 IF NUM(F$(129,5))>NUM(JL$(1,5)) THEN GOTO 6610
6850 IF NUM(F$(129,5))>=NUM(JL$(1,5)) AND NUM(F$(129,5))<=NUM(JL$(6,5)) THEN F$(224,10)=JL$(11,10); GOTO 6590
6860 GOTO 6820
8100 REM "GOSUB here, once, at the beginning, after the background is set and before the Proceed? question. Set T to total # to do (by calling ZZINFO) and T0 to the reporting interval (report every T0 number of records) this is typically 2% of total; This also prints a message (@8115),so adjust accordingly
8110 CALL "ZZINFO",Z[1],T9,X3$,T,T0,K,B,D,S0,S1,F$
8112 T=INT(T0*B/200.0046)
8114 REM 'SUBTRACT 2 , 1 FOR HEADER & 1 FOR TRAILER
8115 PRINT @(25,10),"Rates to load:",@(40,10),STR(T-2:"##,##0"),
8129 REM "Set T0, we make sure T0 is > 1, because later on we MOD and look for avalue of 1. IF T0 is 1, then nothing would get reported. We look fora result of 1 because this causes the first record to automatically start the reporting instead of waiting until the T0'th record to get the first report
8130 T0=INT(T*.004); IF T0<=1 THEN T0=2
8145 RETURN 
8150 REM "Call this each time to update the bar graph. Display horiz. bar graph given total #, T and current #, C, and time counter T1 (T1 is init'ed here and used here, just don't use it somewhere else)
8155 CALL "ZZBARG",X3$,"HG",12,10,50,T1,T,C1
8195 RETURN 
8200 REM "Read first record and print file header information
8205 READ RECORD (Z[1],END=8206)H$; GOTO 8210
8206 CALL "ZZPROM",".4",X3$,0,"There are no records in the file!!!","","",0; GOTO 9900
8210 IF POS("VERTEX 'SALESTAX' TAX RATE FILE"=H$)=0 THEN PRINT @(0,15),H$,; CALL "ZZPROM",".4",X3$,0,"This file does not have the proper Vertex Header Information!!!","","",0; GOTO 9900
8214 REM "H0$ is date of tape, H1$ is update number
8215 H0$=FNE$(NUM(H$(15,2),ERR=8216))+H$(17,4)
8220 H1$=H$(21,4)
8240 PRINT @(25,8),"Update Number: ",@(40,8),H1$,@(25,9),"Date produced: "
8241 CALL "ZZDISP","D",H0$,"",X3$,"","",40,9,X4$
8245 RETURN 
8250 REM "Check out trailer record, set T9$ to "Y" if it is ok
8260 IF POS("9"<>R$(1,10))<>0 OR POS("VERTEX 'SALESTAX' TAX RATE FILE"=R$)=0 THEN PRINT @(0,15),R$,; CALL "ZZPROM",".4",X3$,0,"This file does not have the proper Vertex Trailer Information!!!","","",0; GOTO 8295
8280 T9$="Y"
8295 RETURN 
8920 DEF FNS$(Z9$)=Z9$(1,POS(S$=Z9$+S$)-1)
8930 DEF FNQ$(Z9$,Z9)=STR(NUM(Z9$):Z1$(1,Z9))
8940 DEF FNE$(Z9)=CHR(ASC("A")+INT(Z9/10))+STR(MOD(Z9,10))
8950 DEF FNS(Z9$)=POS(" "<>Z9$)
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9055 REM 
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9900 REM "END PROGRAM
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9930 SETERR 9940; Q1$=A1$; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
