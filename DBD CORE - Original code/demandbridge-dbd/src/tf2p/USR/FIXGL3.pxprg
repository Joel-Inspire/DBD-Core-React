0010 REM "fix gl3 file
0210 OPEN (1)"GL3FIX"
0220 OPEN (2)"GL3474"
2000 REM "REMOVE OLD 1996 RECORDS
2010 K$=KEY(2,END=2100)
2020 READ (2,KEY=K$)
2025 PRINT @(10,18),K$
2030 IF K$(13,4)="1996" THEN REMOVE (2,KEY=K$); PRINT @(10,20),K$
2040 GOTO 2010
2100 REM "WRITE OUT NEW RECORDS
2105 ESCAPE 
2110 READ RECORD (1,END=5000)A$
2120 WRITE RECORD (2,KEY=A$(1,17),DOM=2110)A$
2190 GOTO 2110
