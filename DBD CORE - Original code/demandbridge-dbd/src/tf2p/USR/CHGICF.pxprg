0010 REM "CHANGE RECEIPT DATE IN ICF FILE
0110 DIM A$(270),A[12]
0210 OPEN (1)"ICF500"
0310 IOLIST A$,A[0],A[1],A[2],A[3],A[4],A[5],A[6],A[7],A[8],A[9],A[10],A[11],A[12]
2000 REM "READ FROM ICF TO CHANGE RECEIPT DATE
2010 K$=KEY(1,END=5000)
2020 READ (1,KEY=K$)IOL=0310
2025 PRINT @(10,18),A$(1,32)
2030 IF NUM(A$(21,4))>8999 THEN GOTO 2010
2035 IF STP(A$(1,10),3," ")="" THEN GOTO 2010
2100 REM "CHANGE RECEIPT DATE TO BE 1/1/98
2110 IF NUM(A$(26,5))<71215 THEN A$(25,6)="J71215" ELSE GOTO 2010
2500 REM "WRITE INFO BACK OUT TO ICF FILE
2530 PRINT @(10,20),A$(1,32)
2550 WRITE (1,KEY=A$(1,32),DOM=2600)IOL=0310
2560 REMOVE (1,KEY=K$)
2590 GOTO 2010
2600 REM "INCREMENT SEQ NUMBER
2610 JL=NUM(A$(31,2))+1
2620 A$(31,2)=STR(JL:"00")
2650 GOTO 2550
