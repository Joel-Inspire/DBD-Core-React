0100 BEGIN 
0200 OPEN (1)"FM3524"
0210 OPEN (2)"FM4524"
1000 FOR F=1 TO 2
1001 T=T+1
1100 K$=KEY(F,END=2000); READ (F)
1105 IF K$(21,4)<>"ZZZZ" THEN GOTO 1000
1110 REMOVE (F,KEY=K$)
1120 IF F=1 THEN R1=R1+1 ELSE R2=R2+1
1200 GOTO 1100
2000 NEXT F
3000 PRINT "TOTAL: ",T,"  FM3 CLEARED: ",R1,"  FM4 CLEARED: ",R2
3100 ESCAPE 
