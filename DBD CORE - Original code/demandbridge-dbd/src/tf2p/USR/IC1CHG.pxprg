0010 REM "change location code for certain customers
0210 OPEN (1)"IC1500"
0220 OPEN (2)"IC1500"
2000 REM "READ RECORD
2010 READ (1,KEY="0000500000",DOM=2011)
2020 K$=KEY(1,END=5000)
2025 PRINT @(10,18),K$(1,20)
2030 READ RECORD (1,KEY=K$)A$
2040 IF A$(1,10)<>"0000500000" THEN ESCAPE 
2060 IF A$(21,4)="9001" THEN REMOVE (1,KEY=A$(1,24)) ELSE GOTO 2020
2070 A$(21,4)="9002"
2075 PRINT @(10,20),A$(1,20)
2080 WRITE RECORD (2,KEY=A$(1,24))A$
2090 GOTO 2020
