0010 REM " READ THRU VERTEX FILE AND BUILD ZIP CODE STATE AND CITY SORT
0110 C=0
0210 OPEN (1,ISZ=200)"/usr/lib/pvx/z.kmc/tf2d/tapefile"
0220 OPEN (2)"AR5500"
0230 OPEN (3)"ZYE500"
0320 IOLIST B$
2000 REM "READ THRU VERTEX FILE
2010 READ RECORD (1,END=5000)A$
2020 IF A$(10,1)="0" THEN GOTO 2010
2100 REM "CHECK FOR STATE COUNTY AND CITY RECORD
2110 IF A$(10,1)="1" THEN ZIP$=A$(108,5)+A$(113,5),STATE$=A$(96,2); GOTO 3590
2120 IF A$(10,1)="2" THEN GOTO 3590; REM "LET COUNTY$=A$(24,20); GOTO 03590
2130 IF A$(10,1)="3" THEN GOTO 3590; REM "LET CITY$=A$(24,25)
3500 REM "WRITE OUT TO THE SORT FILE
3550 REM "WRITE (3,KEY=ZIP$+STATE$+CITY$)
3560 PRINT ZIP$," ",STATE$," ",CITY$
3570 C=C+1
3590 GOTO 2010
5000 REM "EOJ
5005 ESCAPE 
5010 GOTO 9900
9900 REM "END
9910 RUN "ZMENU"
