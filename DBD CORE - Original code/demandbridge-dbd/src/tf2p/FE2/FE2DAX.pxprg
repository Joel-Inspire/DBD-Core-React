0010 REM "Calculate Cost Per U/M <FE2DAX>
0190 REM "A$ IS MODEL RECORD
0191 REM "Y$ = SELECTED FEATURES
0200 REM "
0220 ENTER X3$,A$,Y$,A{ALL},F0,M0,X0
0230 IF M0=0 THEN M0=1000
0310 IOLIST Q$,Q[0],Q[1],Q[2],Q[3],Q[4],Q[5],Q[6],Q[7],Q[8],Q[9],Q[10],Q[11],Q[12]
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01X FE1...  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0600 REM 
0620 DIM Q[12]
1000 REM "
1100 REM 
1110 FOR X=0 TO 11; FOR Y=0 TO 1; A[Y,X]=0; NEXT Y; NEXT X
1200 REM "PROCESS FEATURES
1220 FOR N=1 TO LEN(Y$) STEP 3
1240 READ (Z[1],KEY=A$(1,20)+Y$(N,3))IOL=0310
1300 REM "
1320 IF Q$(64,1)="R" THEN FOR X=0 TO 11; A[0,X]=A[0,X]+Q[X],A[1,X]=A[1,X]+Q[12]; NEXT X; GOTO 1400
1330 IF Q$(64,1)="O" THEN FOR X=0 TO 11; A[1,X]=A[1,X]+Q[12]; NEXT X; GOTO 1400
1340 IF Q$(64,1)="F" THEN FOR X=0 TO 11; A[1,X]=A[1,X]+Q[X]; NEXT X; GOTO 1400
1400 REM "
1480 NEXT N
1500 REM "
1590 EXIT 
9999 END 
