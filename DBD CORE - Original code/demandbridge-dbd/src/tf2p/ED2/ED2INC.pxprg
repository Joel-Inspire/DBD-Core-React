2150 PRINT (DEV_CHANNEL)"ST*810*"+Q$(143,9)
2155 IF LEN(FS_HEAD$)>0 THEN ORD_DT$=FND$(FS_HEAD$(16,6)); ORD_DT$=FNF$(ORD_DT$)
2159 IF TRANS_TYPE$="DC" THEN PRINT (DEV_CHANNEL)"BIG*"+DT$+"*"+STP(A$(7,8),1)+"*"+ORD_DT$+"*"+STP(MID(FS_HEAD$,28,15),1)+"***"+BIG_TYPE$; REM SSP 206008
2160 IF TRANS_TYPE$="SN" THEN PRINT (DEV_CHANNEL)"BIG*"+DT$+"*"+STP(A$(7,8),1)+"*"+ORD_DT$+"*"+STP(MID(FS_HEAD$,129,20),1)+"***"+BIG_TYPE$; REM SSP 206008
2161 REM "IF TRANS_TYPE$="CN" THEN PRINT (DEV_CHANNEL)"REF*CM*"+STP(CREDIT$(25, 8),1); CTT=CTT+1;REM OPTIONAL                                              
2162 PRINT (DEV_CHANNEL)"CUR*BY*CAD*"; CTT=CTT+1 ! 294165 SSP 307507:DBD-496
2163 PRINT (DEV_CHANNEL)"REF*IA*"+STP(NSID$,2); CTT=CTT+1 ! SSP 307498:DBD-489SSP 307507
2164 PRINT (DEV_CHANNEL)"N1*RI*"+"Centurion Store Supplies"+$0A$+"N3*"+"188 Industrial Drive Suite 428"+$0A$+"N4*"+"Elmhurst*"+"IL*60126*US"; CTT=CTT+3 ! DBD-489 SSP 307498
2165 REM "IF TRANS_TYPE$="CN" THEN PRINT (DEV_CHANNEL)"REF*CD*"+STP(G$(185,8),1); CTT=CTT+1; REM " RECEIVING DEPARTMENT- LOOKS LIKE WHERE THEY PUT CREDIT AUTH  #                                                                         
2166 REM "IF TRACKING_NO$<>"" THEN PRINT (DEV_CHANNEL)"REF*CN*"+STP(TRACKING_NO $,1); CTT=CTT+1; ELSE IF TRACKING_NO$="" THEN PRINT (DEV_CHANNEL)"REF*CN*N ONE"; CTT=CTT+1                                                            
2168 REM "IF TRANS_TYPE$="SN" THEN TRADE_PART$=EM1$(51,10); PRINT (DEV_CHANNEL)"REF*CO*"+STP(A$(100,15),1)+"*"+STP(TRADE_PART$,1); CTT=CTT+1; REM OPTIONAL                                                 
2169 REM "IF POS("/R"=A$(290,35))>0 THEN PRINT (DEV_CHANNEL)"N1*RI*"+STP(Q$(22,30),2)+"*92*"+"0040889546"+$0A$+"N3*"+STP(Q$(52,30),2)+$0A$+"N4*"+STP(Q$(112,20),2)+"*"+STP(Q$(132,2),2)+"*"+STP(Q$(134,9),2); REM 174682
2170 WHSE=POS("WD"=A$(290,35)); WHSE$=A$(290+WHSE+1,4)
2171 IF TRANS_TYPE$="DC" THEN PRINT (DEV_CHANNEL)"N1*ST*"+STP(A$(290,35-WHSE-6),2)+"*92*"+STP(WHSE$,2)+$0A$+"N3*"+STP(A$(191,30),2)+$0A$+"N4*"+STP(A$(251,16),2)+"*"+A$(267,2)+"*"+STP(A$(269,9),2); CTT=CTT+3 ! DBD-400-DBSPT-164641-SSP307452
2172 REM IF TRANS_TYPE$="SN" THEN PRINT (DEV_CHANNEL)"N1*SO*"+STP(G$(11,35),2)+"*92*006929681"; CTT=CTT+1;REM SSP 206008
2173 IF TRANS_TYPE$="SN" THEN PRINT (DEV_CHANNEL)"N1*ST*"+STP(G$(11,35),2)+"*92*"+STP(G$(185,20),2)+$0A$+"N3*"+STP(G$(56,30),2)+$0A$+"N4*"+STP(G$(116,16),2)+"*"+G$(132,2)+"*"+STP(G$(134,9),2); CTT=CTT+3; REM SSP 206008 ! DBD-400-DBSPT-164641-SSP307452
2174 REM IF TRANS_TYPE$="SN" THEN PRINT (DEV_CHANNEL)"N1*ST*"+STP(A$(303,22),2)+"*92*"+STP(A$(290,13),2); CTT=CTT+1; REM SSP 206008
2175 FIND (Z[30],KEY=A$(65,2),DOM=2172)TERM$,TERM_PERCENT
2176 DISC_DUE_DT$=FND$(A$(122,6)); DISC_DUE_DT$=FNF$(DISC_DUE_DT$)
2177 DISC_AMT=A[10]*100
2178 DUE_DT$=FND$(A$(116,6)); DUE_DT$=FNF$(DUE_DT$)
2179 PRINT (DEV_CHANNEL)"ITD**3****"+STP(DUE_DT$,1)+"*20*****NET 20 Days"; CTT=CTT+1; REM ELSE PRINT (DEV_CHANNEL)"ITD**3***"+STP(DT$,1)+"*"; CTT=CTT+1;REM SSP 206008 SSP 307498 DBD-498
2180 PRINT (DEV_CHANNEL)"DTM*011*"+FNF$(FND$(A$(180,6)))
2190 RETURN 
2200 REM "Write line items out to EDI file
2210 KT=A[20],TRACK_NO$=""
