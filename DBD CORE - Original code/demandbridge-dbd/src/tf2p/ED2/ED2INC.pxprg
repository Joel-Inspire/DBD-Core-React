2131 REM "PRINT (DEV_CHANNEL)Q$(7,15)+"*"+EM1$(152,2)+"*"+REC_ID$+"*"+DTT$(3)+"*"+TIME$+"*U*"+STP(EM1$(157,10),1)+"*"+Q$(143,9)+"*0*"+TEST$+"*>"
