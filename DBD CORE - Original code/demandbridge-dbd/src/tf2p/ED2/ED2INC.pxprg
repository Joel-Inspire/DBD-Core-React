2322 IF TRANS_TYPE$="CN" THEN PRINT (DEV_CHANNEL)"TDS*"+STP(TDS$,0,"0"); CTT=CTT+1
2325 IF TRACK_NO$="" THEN TRACK_NO$="NA"
