2346 IF TAX_TOT<>0 THEN PRINT (DEV_CHANNEL)"SAC*"+"C*H750***"+STP(STR(TAX_TOT:M$),2)+"*******02***TAX_SALES TAX(STATE AND LOCAL)"; CTT=CTT+1
