0010 REM "Parse Macro Parameter List <ZA2DU1>
1020 ENTER X3$,V0$,V8,V9,M0$,M9$,D0$,D1$,D2$,D0,D3$,E0$,E1$,E2$
1120 DIM S$(60)
1130 D0$=""
2000 REM "PARSE PARAMETER LIST
2005 Q9$=M0$
2010 IF POS(" "<>Q9$)=0 THEN GOTO 2200
2020 CALL "ZA2UAB",Q9$,Q$,Q9
2025 IF Q$="," THEN GOTO 2020
2030 IF Q$(LEN(Q$),1)="," THEN Q$(LEN(Q$),1)=""
2035 P=POS(":"=Q$); IF P=0 THEN CALL "ZZPROM","X0ZA2DUA",X3$,A,"","","",0; GOTO 9900
2100 REM "ADD TO VAR LIST
2110 IF LEN(Q$)>20 THEN CALL "ZZPROM","X1ZA2DUA",X3$,A,"","","",0; GOTO 9900
2115 R9=0; IF POS("RESULT:"=Q$)=1 THEN R9=1; Q$="RESULT^"+M9$+Q$(7)
2120 CALL "ZA2BU5",X3$,Q$,V0$,V8,V9,I9$,I8$,E0$,E1$,E2$
2130 IF R9>0 THEN D3$=I9$
2135 D0$=D0$+I9$+","
2140 GOTO 2010
2200 REM "DONE
9940 EXIT 
9999 END 
