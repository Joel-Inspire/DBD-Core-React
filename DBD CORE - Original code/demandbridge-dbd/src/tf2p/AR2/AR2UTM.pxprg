0600 REM "
0610 GOSUB 6000
0640 CALL "ZZPROM",".Y",X3$,Z,"Proceed?","","",0; ON Z GOTO 0641,9900
0700 DIM A$(7)
0710 CALL "ZZENTR","Z",A{ALL},A$,X4$,X3$,35,8,1,4,C0,"",X$,"","AR2UTM00","","",""; IF ABS(C0)>=4 THEN GOTO 9900
