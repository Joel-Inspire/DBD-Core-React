10320 DELETE_TIERED_APPROVER_GROUP:
10330 ! 
10340 ! Deletes all EE9 records for tiered approver group
10350 ! 
10360 SEG2$="",SEG3$="",SEG4$="",SEG5$=""
10370 DIM Z[255],A[1]
10380 ENTER EE9,GROUP_KEY$
10390 Z$="01O EE9...  "; IF POS("EE9"=PTH(EE9))=0 THEN GOSUB INIT ELSE Z[1]=EE9,Z$=""
10400 CALL "UPDEE9;READBYKEY",Z[1],0,GROUP_KEY$,SEG2$,SEG3$,SEG4$,SEG5$,A$,A{ALL},FOUND
10410 CALL "UPDEE9;READNEXT",Z[1],A$,A{ALL},FOUND; IF FOUND THEN IF A$(1,14)<>GROUP_KEY$ THEN FOUND=0
10420 WHILE FOUND
10430 CALL "UPDEE9;DELETE",Z[1],A$,A{ALL},FOUND,BUSY
10440 CALL "UPDEE9;READNEXT",Z[1],A$,A{ALL},FOUND; IF FOUND THEN IF A$(1,14)<>GROUP_KEY$ THEN FOUND=0
10450  }
10460 WEND 
10470 IF Z$<>"" THEN GOSUB WRAPUP
10480 EXIT 
