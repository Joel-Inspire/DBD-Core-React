0610 GOSUB 6000
1000 REM "MAIN LOOP
1010 K$=KEY(Z[1],END=9900); READ RECORD (Z[1],KEY=K$)IL1$
1020 IF POS(IL1$(6,2)="070809",2)=0 THEN GOTO 1000
1030 READ (F0,KEY=IL1$(213,20),DOM=1000)FSS$
1040 IL1$(213,20)=FSS$(21,20)
1050 WRITE RECORD (Z[1],KEY=K$)IL1$
1090 GOTO 1000
6000 REM "BACKGROUND                                                            
6005 IF %GUI=0 THEN PRINT (0,ERR=6016)'SB',
6020 IF %GUI=0 THEN PRINT @(0,4),"This utility program Updates the Image Library Masterfile during a Item Number Change",
6165 IF %GUI=0 THEN PRINT (0,ERR=6166)'SF',
6190 RETURN 
7999 RETURN 
