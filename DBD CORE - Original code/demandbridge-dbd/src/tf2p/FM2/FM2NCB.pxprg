0045 ! F0=device FSS is on
0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,F0
0100 SETERR 9000
0110 X0$="FM2NCB",X1$="Image Library item number change"
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0250 PRECISION 9
0300 REM "IOLISTS
0500 REM "FILES                                                         
0505 DIM Z[NUM(X3$(60,3))]
