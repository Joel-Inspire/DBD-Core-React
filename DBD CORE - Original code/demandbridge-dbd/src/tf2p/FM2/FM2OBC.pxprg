0010 REM "Create Item Master from E/Q Order Line <FM2OBC>
0035 REM "4.1 - 02/14/97 - 13.33 - kmc
0040 REM "Copyright 1997 Computer Software Inc.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0090 SETERR 9000
0095 ENTER X3$,X4$,A$,A{ALL},B$
0110 X0$="FM2OBC"
0330 IOLIST C$,C[0],C[1],C[2],C[3],C[4],C[5],C[6],C[7],C[8],C[9],C[10],C[11],C[12],C[13],C[14],C[15],C[16],C[17],C[18],C[19],C[20],C[21],C[22],C[23],C[24],C[25],C[26],C[27],C[28],C[29],C[30],C[31]
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="03O FM1...  05O FM2...  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0585 RT_PARM$=""; CALL "RT2PRM",ERR=0586,X3$,X4$,RT_PARM$
4000 REM "Set up Item Master for E/Q
4020 READ (Z[3],KEY=B$(6,10)+A$(19,10),DOM=4021); GOTO 9900
4030 DIM C[31],C$(448)
4040 C$(1,20)=B$(6,10)+A$(19,10),C$(21,61)=A$(29,61),C$(82,14)=A$(94,10)+A$(90,4),C$(96,8)=A$(120,8),C[2]=A[2],C[4]=A[5]
4070 WRITE (Z[3],KEY=C$(1,20),DOM=4071)IOL=0330; IF RT_PARM$<>"" THEN CUST$=C$(1,10); CALL "RT2WOC",ERR=4071,X3$,X4$,CUST$,"FM1...","U",C$(1,20)
4080 WRITE (Z[5],KEY=C$(21,3)+C$(24,16)+C$(40,2)+C$(11,10)+C$(1,10))
4990 GOTO 9900
9000 REM "ERROR PROCESSING
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 RETURN 
9900 REM 
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9940 EXIT 
9999 END 
