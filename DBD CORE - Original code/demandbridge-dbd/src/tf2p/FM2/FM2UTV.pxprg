0120 DIM Z0$(80,"-"),S$(40)
0130 K0=20,K1=1
0135 C9=-1
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=MNM('GS')+Z0$+MNM('GE')
0300 REM "IOLISTS
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01O FSF...  02O FM1... 03O FMP...  04O FSE...  13O ZZPARM  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0600 REM "
0610 GOSUB 6000
0640 IF Q1$<>"*" THEN CALL "ZZPROM",".Y",X3$,Z,"Proceed?","","",0; ON Z GOTO 0641,9900
0980 READ (Z[1],KEY="",DOM=0981)
0995 PRINT @(20,10),'CE',"Processing cust/item:",
1000 REM 
1010 K$=KEY(Z[1],END=5000); READ (Z[1],KEY=K$)A$
1020 PRINT @(43,10),A$(1,10),"  ",A$(11,10),
1040 FIND (Z[2],KEY=A$(1,20),DOM=1000)B$
1050 FIND (Z[3],KEY="T"+B$(21,3),DOM=1000)C$
1060 READ (Z[4],KEY=C$(33,3),DOM=1061)
1080 K7$=KEY(Z[4],END=1000); IF K7$(1,3)<>C$(33,3) THEN GOTO 1000
1220 EXTRACT RECORD (Z[1],KEY=K$)A$
1240 A$(21,3)=C$(33,3)
1260 WRITE RECORD (Z[1])A$
1290 GOTO 1000
5000 REM "EOJ
5020 IF Q1$<>"*" THEN CALL "ZZPROM",".4",X3$,Z,"Process complete!!","","",0
5040 GOTO 9900
6000 REM "BACKGROUND
6005 PRINT (0,ERR=6016)'SB',
6020 PRINT @(0,4),"This CSI utility program processes the Custom Item User defined spec records and plugs the template code into positions 21,3 in file FSF.",
6165 PRINT (0,ERR=6166)'SF',
6190 RETURN 
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
