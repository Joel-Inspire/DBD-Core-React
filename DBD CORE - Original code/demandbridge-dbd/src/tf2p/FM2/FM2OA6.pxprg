0090 CLEAR ; SETERR 0100; ENTER X3$,C0,A$
0100 SETERR 9000
0110 X0$="FM2OA6"
0210 IF NOT(%GUI) THEN PRINT @(23,22),"Updating RFQ file", ! SSP#213461
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="02O QSA...  03O FS2...  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
1000 REM 
1010 C1=SGN(C0)
1015 IF A$(183,1)="Y" THEN GOTO 2000; REM "SSP98716
1020 EXTRACT RECORD (Z[2],KEY=A$(23,5)+A$(3,3),DOM=9900)B$
1040 IF C1>0 THEN B$(455,8)=A$(118,8),B$(463,6)=A$(16,6)
1060 IF C1<0 AND B$(455,14)=A$(118,8)+A$(16,6) THEN B$(455,14)=""
1100 WRITE RECORD (Z[2])B$
1900 GOTO 9900
2000 REM "More than one RFQ per order, read FS2 and do any we find
2010 READ (Z[3],KEY=A$(118,8),DOM=2011)
2015 K$=KEY(Z[3],END=9900); IF K$(1,8)<>A$(118,8) THEN GOTO 9900
2020 READ RECORD (Z[3],KEY=K$,END=9900)FS2$
2025 IF STP(FS2$(221,8),3," ")="" THEN GOTO 2015
2030 EXTRACT RECORD (Z[2],KEY=FS2$(221,8),DOM=2015)B$
2035 IF C1>0 THEN B$(455,8)=A$(118,8),B$(463,6)=A$(16,6)
2040 IF C1<0 AND B$(455,14)=A$(118,8)+A$(16,6) THEN B$(455,14)=""
2045 WRITE RECORD (Z[2])B$
2050 GOTO 2015
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9900 REM "END PROGRAM
9905 IF NOT(%GUI) THEN PRINT @(0,22),'CL', ! SSP#213461
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9930 SETERR 9940; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
