0045 ! New program WO252273 - when FS1 or PO1 are deleted we will write a record to FV2 or FV3 recording the FS1/PO1 data plus op code, date/time
0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,Q0$,Q1$
0099 ! 
0100 INIT:SETERR 9000
0105 GOTO 0500 ! Not sure if we need anything else but file opens
0110 X0$="FM2DEL",X1$="Record FS1/PO1 Deletes"
0120 DIM Z0$(80,"-")
0130 K0=20,K1=1
0135 C9=-1
0200 REM "                                                                 
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=ESC+"BG"+Z0$+ESC+"EG"
0300 REM "IOLISTS                                                          
0310 IOLIST A$
0320 IOLIST B$
0500 REM "FILES                                                            
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01O FV2...  02O FV3...  13O ZZPARM  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0590 INIT_DONE:RETURN 
0595 ! 
1000 WRITE_FV2:! Called from FS1 delete to write FV2 record
1010 CLEAR ; SETERR 1015; ENTER X3$,X4$,FS1$,FS1{ALL},OTHER$
1015 GOSUB INIT
1020 FS1$=PAD(FS1$,606)
1025 FV2_KEY$=FS1$(118,8); DIM FV2$(706),FV2[13]
1030 FV2$(1,3)=X3$(40,3)
1035 FV2$(4,6)=X3$(21,6)
1040 FV2$(10,4)=STR(INT(TIM):"00")+STR(100*FPT(TIM):"00")
1045 FV2$(101,606)=FS1$,FV2{ALL}=FS1{ALL}
1080 WRITE (Z[1],KEY=FV2_KEY$)FV2$,FV2{ALL}
1090 DONE_WRITE_FV2:GOTO 9900
1095 ! 
1100 WRITE_FV3:! Called from PO1 delete to write FV3 record
1110 CLEAR ; SETERR 1115; ENTER X3$,X4$,PO1$,OTHER$
1115 GOSUB INIT
1120 PO1$=PAD(PO1$,383)
1125 FV3_KEY$=PO1$(297,9); DIM FV3$(483)
1130 FV3$(1,3)=X3$(40,3)
1135 FV3$(4,6)=X3$(21,6)
1140 FV3$(10,4)=STR(INT(TIM):"00")+STR(100*FPT(TIM):"00")
1145 FV3$(101,383)=PO1$
1180 WRITE (Z[2],KEY=FV3_KEY$)FV3$
1190 DONE_WRITE_FV3:GOTO 9900
1195 ! 
