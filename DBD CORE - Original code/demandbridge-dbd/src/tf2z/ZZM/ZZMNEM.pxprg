0010 REM "Mnemonics interface <ZZMNEM>
0030 REM "(C) 1985,1986,1987 Basic Ideas, Inc; Atlanta, Ga.
0035 REM "This software contains trade secrets and proprietary information"
1000 REM T$=TCONFIG RECORD, FO=KEYED FILE, F0$=TABLE NAME
1010 SETERR 1025
1020 ENTER X3$,T$,X0,F0,F0$,A0,A0$; GOTO 1030
1025 IF X3$="?" THEN CALL "ZZPDOC","ZZMNEM"; EXIT ELSE EXIT ERR
1030 SETERR 9000; L0=5,L1=X0
1035 P=POS($FF13$=T$),X=DEC(T$(P+2,2)),T0$=T$(P+4,X)
1040 GOSUB 2000
1090 EXIT 
1100 REM 
1120 PRINT @(0,L1),'CE',
1150 X=ASC(T0$(4,1)),Q$=T0$(10,X-6)
1200 PRINT @(L0,L1),T0$(2,2)," ",Q$,
1210 L1=L1+1; IF L1=19 THEN INPUT "PRESS RETURN FOR MORE: ",*; PRINT @(0,X0),'CE',; L1=X0
1300 REM 
1320 T0$=T0$(X+4)
1330 IF LEN(T0$)>0 THEN GOTO 1100 ELSE RETURN 
2000 REM "COPY RECORDS INTO FILE F0
2020 X=ASC(T0$(4,1)),Q$=T0$(10,X-6)
2040 DIM E$(50); E$(1,8)=F0$
2045 E$(9,2)=T0$(2,2)
2050 E$(46,5)=T0$(5,5)
2055 E$(11,35)=Q$
2080 WRITE (F0,KEY=E$(1,10))E$
2085 T0$=T0$(X+4); IF T0$>"" THEN GOTO 2000
2095 RETURN 
9000 REM "ERROR PROCESSING
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9055 REM 
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9800 REM "EXIT PROGRAM
9900 REM "END PROGRAM
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9930 SETERR 9940; Q1$=A1$; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
