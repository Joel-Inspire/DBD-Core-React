0010 REM "Drop Public Progs in my Bank <ZZDROP>
0015 EXIT ! <mk>
0600 SETERR 0620; ENTER Q0$
0620 SETERR 9000
0800 REM "
0810 SETERR 0840; ENTER L0$,L1$
0820 SETERR 9000
1000 REM "FIND MY BANK
1020 D0$=FID(0),B0=0,P$=PGN
1100 REM "
1110 B0=B0+1
1120 B0$=TSK(B0); IF B0$="" THEN GOTO 1100
1130 P=POS(D0$=B0$(5),6); IF P=0 THEN GOTO 1100
1200 REM "PUBLIC PROGS IN BANK B0
1210 P0$=PUB(B0); IF P0$="" THEN GOTO 9900 ELSE D0=DSZ+PSZ
1220 FOR X=1 TO LEN(P0$) STEP 12
1230 Q$=P0$(X+4,8); IF P$=Q$ THEN GOTO 1290
1235 IF POS(Q$=L0$,8)>0 THEN GOTO 1290
1240 DROP Q$,ERR=1241; GOTO 1240
1250 IF POS("D"=L1$)>0 THEN PRINT @(24,22),"NOW REMOVING RESIDENT PROGRAM ",Q$,; Q9$="*"
1290 NEXT X
1400 REM "
1410 IF POS("D"=L1$)>0 THEN IF Q9$>"" THEN PRINT @(0,22),'CL',@(12),"MEMORY INCREASED FROM ",D0," TO ",DSZ+PSZ,; INPUT *,@(0,22),'CL',
9980 SETERR 9990; EXIT 
9990 END 
