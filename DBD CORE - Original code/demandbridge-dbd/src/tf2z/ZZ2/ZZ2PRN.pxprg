0010 REM "Concatenate data for PRN output <ZZ2PRN>
1020 ENTER X3$,X0$,Q$,N,Y5$
1025 DIM S$(40)
1030 T0$=$0D0A$,T1$=$22$
1040 N$=""; DIM M0$(14,"0"); M0$="-0."+M0$
1090 ON LEN(Q$) GOTO 2000,4000
1545 Y5$=Y5$(1,LEN(Y5$)-1)
2000 N$=STR(N); GOTO 2100
2010 FLOATING POINT 
2020 I=INT(N),F=FPT(N)
2050 E=EPT(N)-1
2060 E0=10^E
2070 N0=N/E0
2080 N$=STR(N0:M0$)+"E"+STR(E:"+00")
2100 REM 
2190 GOTO 8000
4000 REM "STRING
4005 REM IF LEN(Q$)>12 THEN LET Q$=FNS$(Q$)
4010 N$=T1$+Q$+T1$
4090 GOTO 8000
8000 REM "ADD TO STRING
8010 Y5$=Y5$+N$+","
8090 GOTO 9900
8910 DEF FNS$(Z$)=Z$(1,POS(S$=Z$+S$)-1)
9940 EXIT 
