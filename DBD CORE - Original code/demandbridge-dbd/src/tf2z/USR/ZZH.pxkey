   <RecordData>
    ExternalKey$=" ZP2RBS22 "
    FIELD_1$=" ZP2RBS22 Selection Codes to Include    When codes are specified, any contact must have ALL the codes         specified in their masterfile.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
