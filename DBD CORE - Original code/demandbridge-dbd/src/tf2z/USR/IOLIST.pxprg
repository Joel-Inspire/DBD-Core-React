1010 INPUT "ENTER LINE NUMBER: ",L0
1020 INPUT "ENTER STRING VARIABLE: ",L0$
1025 REM "IF LEN(L0$)>2 THEN LET Q$=L0$; GOTO 02000
1040 INPUT "ENTER NUMERIC VARIABLE: ",L1$
1045 INPUT "ENTER HIGHEST NUMERIC: ",L1
1050 IF L1$="" THEN L1$=L0$
1200 REM 
1220 Q$=STR(L0)+" IOLIST "+L0$+"$"
1230 IF L1>0 THEN FOR X=0 TO L1; Q$=Q$+","+L1$+"["+STR(X)+"]"; NEXT X
1235 PRM_EX=PRM('EX'); SET_PARAM 'EX'
1240 EXECUTE Q$
1245 SET_PARAM 'EX'=PRM_EX
1290 GOTO 9900
2000 REM "GET FILE DATA
2010 CLOSE (14); OPEN (14)"ZZE"
2015 Q$=Q$+"      ",Q$=Q$(1,6)
2020 READ (14,KEY=Q$,DOM=2021)F$; GOTO 2100
2030 PRINT "FILE: ",Q$," NOT FOUND."
2040 GOTO 1020
2110 CLOSE (14)
2120 IF L0>300 AND L0<500 THEN Q=L0-300,Q=INT(Q/10)
2140 L0$=CHR(ASC("A")+Q-1),L1$=L0$
2150 L1=NUM(F$(67,3),ERR=1020)
2190 GOTO 1200
9940 EXIT 
9999 END 
