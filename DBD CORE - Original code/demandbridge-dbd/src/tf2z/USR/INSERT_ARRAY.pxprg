0010 ! INSERT_ARRAY
0015 REM "Modified by dmm - 1/10/02
0100 STRING:! ^100
0110 ENTER X${ALL},AT,N
0120 L=DIM(READ MIN(X$))
0130 T=DIM(READ MAX(X$))
0140 DIM Y$[L:T]
0150 Y${ALL}=X${ALL}
0160 DIM X$[L:T+N]
0165 X${L:T}=Y${L:T}
0170 REM IF AT>L THEN X${L:AT-1}=Y${L:AT-1}
0180 REM IF AT<T THEN X${AT+N:T+N}=Y${AT:N}
0190 EXIT 
0200 NUMERIC:! ^100
0210 ENTER X{ALL},AT,N
0220 L=DIM(READ MIN(X))
0230 T=DIM(READ MAX(X))
0240 DIM Y[L:T]
0250 Y{ALL}=X{ALL}
0260 DIM X[L:T+N]
0265 X{L:T}=Y{L:T}
0270 REM IF AT>L THEN X{L:AT-1}=Y{L:AT-1}
0280 REM IF AT<T THEN X{AT+N:T+N}=Y{AT:N}
0290 EXIT 
