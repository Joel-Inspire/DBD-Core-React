2140 GOTO 2020
2160 CALL "ZZENTR","SU",A{ALL},A$,X4$,X3$,24,8,53,3,C0,"",X$,"","ZZXFBA08","","",""
2190 GOTO 2020
2210 CALL "ZZENTR","Z",A{ALL},A$,X4$,X3$,24,9,56,3,C0,"",X$,"","ZZXFBA10","","",""
2240 GOTO 2020
2260 CALL "ZZENTR","SU",A{ALL},A$,X4$,X3$,51,8,59,9,C0,"",X$,"","ZZXFBA12","","",""
2290 GOTO 2020
2310 CALL "ZZENTR","SU",A{ALL},A$,X4$,X3$,51,9,68,8,C0,"",X$,"","ZZXFBA14","ZZE","ZZXFAA",""; IF C0=6 THEN GOSUB 6400; C0=1
2320 FIND (Z[3],KEY=A$(68,6),DOM=2321)IOL=0330
2340 GOTO 2020
2360 CALL "ZZENTR","SU",A{ALL},A$,X4$,X3$,24,11,76,16,C0,"",X$,"","ZZXFBA16","","",""
2390 GOTO 2020
2410 CALL "ZZENTR","S",A{ALL},A$,X4$,X3$,24,13,92,15,C0,"",X$,"","ZZXFBA18","","",""
2440 GOTO 2020
2460 CALL "ZZENTR","S",A{ALL},A$,X4$,X3$,24,14,107,15,C0,"",X$,"","ZZXFBA20","","",""
2490 GOTO 2020
6000 REM "BACKGROUND
6015 PRINT (0,ERR=6006)'SB',
6060 PRINT @(0,3),"File Name:",@(0,4),"Data Type:",@(18,4),"String Position or Array Index:",@(0,5),Z0$,@(0,6),"Data Field Description:",@(13,8),"Data Type:",@(37,8),"Display Type:",@(3,9),"Maximum Data Length:",@(32,9),"Verification File:",@(10,11),"Data Synonym:",@(7,13),"Report Column 1:",@(7,14),"Report Column 2:",
6065 PRINT (0,ERR=6066)'SF',
