1205 IF LEN(A1$)>0 THEN A$(1,6)=A1$(1,6); GOTO 1215
1210 CALL "ZZENTR","SXUX",A{ALL},A$,X4$,X3$,11,3,1,6,C0,"",X$,K9$,"ZZXFBA00","ZZE","ZZXFAA",""; IF C0=6 THEN GOSUB 6400; C0=1
1215 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1216,6800,9800
