1310 CALL "ZZENTR","SZUX",A{ALL},A$,X4$,X3$,50,4,10,3,C0,"",X$,Q$,"ZZXFBA04","ZZF","",""; IF C0=6 THEN GOSUB 6400; C0=1 ! SSP177776, was "ZUX", changed to "SZUX" so we can use alpha for starting positions over 999, A00=1000, B00=1100, and so on.
1312 IF ABS(C0)=2 AND LEN(A1$)>6 THEN A1$=A1$(1,6)
1315 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1216,6800,9800
