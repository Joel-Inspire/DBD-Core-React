1260 CALL "ZZENTR","SUX",A{ALL},A$,X4$,X3$,11,4,7,3,C0,"",X$,Q$,"ZZXFBA02","ZZF","",""
1262 IF ABS(C0)=2 AND LEN(A1$)>0 THEN A1$=A1$(1,0)
1265 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1216,6800,9800
1290 GOTO 1120
