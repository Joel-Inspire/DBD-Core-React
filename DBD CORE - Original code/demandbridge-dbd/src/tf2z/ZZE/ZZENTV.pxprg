3520 Q$=""; CALL "ZZINPT","",X3$,S,X0$+C0$,Q$,X2$,X0,X1,X4,Q,X8$,X7$,J9$,L0$,L1$,L2$,L3$,X6$,"",""; IF X4=5 AND X0$<>"X" THEN GOTO 8000 ELSE IF X4=.1 THEN EXIT 65
