0010 REM "Data Filtering Test Routine <ZZFLTR>
0030 REM "(C) 1985,1986,1987 Basic Ideas, Inc; Atlanta, Ga.
0035 REM "This software contains trade secrets and proprietary information"
1000 REM 
1010 REM "F0$=MAIN MASKING, F1$=SUB MASKING, F2$=DATA, F0=RESULT(1=YES,0=NO), F1=WARNING
1020 ENTER X3$,F0$,F1$,F2$,F0,F1
1030 F0=0,F1=0,F0$=FNS$(F0$)
1200 REM 
1220 Q0=LEN(F0$); IF LEN(F1$)>Q0 THEN Q0=LEN(F1$)
1300 FOR X=1 TO Q0
1310 IF X<=LEN(F0$) THEN IF F0$(X,1)="*" THEN GOTO 1400
1320 IF X<=LEN(F0$) THEN IF F0$(X,1)<>"?" THEN IF F2$(X,1)<>F0$(X,1) THEN EXITTO 2000
1400 REM "SUB FILTERING
1420 IF F1$="" THEN GOTO 1500 ELSE IF F1$(X,1)="*" THEN GOTO 1800
1440 IF F1$(X,1)<>"?" THEN IF F2$(X,1)<>F1$(X,1) THEN EXITTO 2000
1800 REM 
1810 NEXT X
1840 F0=1
2000 REM 
2090 GOTO 9900
8910 DEF FNS$(Z$)=Z$(1,POS("  "=Z$+"  ")-1)
9900 REM 
9940 EXIT 
9999 END 
