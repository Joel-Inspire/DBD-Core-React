0100 BEGIN 
0105 C0$="500"
0110 OPEN (1)"PO3"+C0$
0120 OPEN (2)"FM0"+C0$
0130 OPEN (3)"FS1"+C0$
0140 OPEN (4)"AR1"+C0$
0310 IOLIST A$(1),A[0],A[1],A[2],A[3]
1000 REM "PO3 - **CHANNEL 1**
1020 K$=KEY(1,END=2000)
1030 DIM A$(259),A[3]
1040 READ (1,KEY=K$)IOL=0310
1050 PRINT "PO3-",A$(166,9),"-",A$(7,3),"-",A$(10,1)
1060 IF A$(11,1)<>"C" OR A$(174,1)<>" " OR A$(12,4)="M   " THEN GOTO 1000
1080 FIND (3,KEY=A$(166,8),DOM=1000)F$
1100 FIND (4,KEY=F$(6,10),DOM=1000)C$
1120 A$(225,35)=C$(11,35)
1140 WRITE (1,KEY=K$)IOL=0310
1160 GOTO 1000
2000 REM "FM0 - **CHANNEL 2**
2010 READ (2,KEY="C",DOM=2011)
2020 K$=KEY(2,END=9900)
2030 IF K$(1,1)>"C" THEN GOTO 9900
2040 DIM A$(321); READ (2,KEY=K$)A$(1)
2050 PRINT "FM0-",A$(2,10),"-",A$(12,4)
2060 FIND (4,KEY=A$(2,10),DOM=2020)C$
2080 A$(248,35)=C$(11,35)
2100 WRITE (2,KEY=K$)A$
2120 GOTO 2020
9900 PRINT "EOJ"
9999 END 
