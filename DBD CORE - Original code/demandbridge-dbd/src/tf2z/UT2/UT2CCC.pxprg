0010 REM 
0100 BEGIN 
0105 INPUT "ENTER PROGRAM NAME: ",F0$; GOTO 1021
1010 REM "F0$=PROG NAME, F9=1 MEANS FAST CHECK
1015 REM "V9=0 MEANS DISPLAY, V9>1 = DEVICE
1020 ENTER X3$,F0$,F1,F9,V9
1030 A0$="",A1$=""
1050 IF V9=0 THEN PRINT 'T2'
1060 DIM Y5$(131)
1090 Z9=50
1900 REM 
1910 ENABLE 0,ERR=1911
1920 ENABLE 1,ERR=1921
2000 REM 
2010 DISABLE 1
2020 CALL "ZZ2BAY",F0$,0,9999,14,A0$; CLOSE (14)
2030 ENABLE 1; DISABLE 0
2040 CALL "ZZ2BAY",F0$,0,9999,14,A1$; CLOSE (14)
2060 ENABLE 0
3000 REM "PRINT DIFFERENCES
3005 IF A0$="" AND A1$="" THEN GOTO 5000
3010 C0=0,C1=0; IF A0$>"" THEN C0=DEC($00$+A0$(4,2))
3012 IF A1$>"" THEN C1=DEC($00$+A1$(4,2))
3020 IF C0<=C1 THEN B0=DEC(A0$(1,3)),B0$=A0$(4,B0),A0$=A0$(B0+4),C0$=LST(B0$) ELSE C0$="",B0$=""
3040 IF C1<=C0 THEN B1=DEC(A1$(1,3)),B1$=A1$(4,B1),A1$=A1$(B1+4),C1$=LST(B1$) ELSE C1$="",B1$=""
3044 IF C0$>"" THEN C0$=C0$(7)
3045 IF C1$>"" THEN C1$=C1$(7)
3050 IF B0$=B1$ THEN GOTO 3000
3060 Y5$(9,5)=STR(C0:"00000"),Y5$(8+Q1*Z9)=STR(C1:"00000")
3070 Q1=0,Q$=C0$; GOSUB 3400; C0$=Q$
3080 Q1=1,Q$=C1$; GOSUB 3400; C1$=Q$
3090 PRINT Y5$; Y5$(1)=""
3095 IF C0$+C1$<>"" THEN GOTO 3070 ELSE GOTO 3000
3400 REM "LIST OUT Q$,Q0=LINE, Q1=0(LEFT), 1(RIGHT)
3410 IF LEN(Q$)<Z9 THEN X$=" "+Q$,Q$="" ELSE X$=":"+Q$(1,Z9),Q$=":"+Q$(Z9+1)
3420 Y5$(8+Q1*(Z9+9),LEN(X$))=X$
3490 RETURN 
5000 REM "END
5090 GOTO 9900
8910 DEF FNA(A0)=DEC(A0$(1,3))
9999 END 
