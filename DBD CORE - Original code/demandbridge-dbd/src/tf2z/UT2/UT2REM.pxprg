0010 REM "GET RID OF REMS IN A PROGRAM <UT2REM>
0050 PRINT 'CS',"YOU MUST MERGE $$ FILE AFTERWARD"
0100 BEGIN ; ENTER F9$
0110 INPUT "ENTER THE PROG: ",Q$; IF Q$="" THEN GOTO 9900 ELSE OPEN (1,ERR=0110)Q$; Q$=FID(1); IF LEN(Q$)=2 THEN GOTO 0100
0115 IF Q$(10,1)<>$04$ THEN GOTO 0110 ELSE F9$=Q$(4,6)
0120 OPEN (2)"$$"
0400 READ RECORD (1,IND=0)A$
0420 A=DEC(A$(1,3))
0440 READ RECORD (1,IND=0)B$
0450 B$=B$(13+ASC(B$(12,1))*3)
1000 REM "STUFF
1010 IF B$="" THEN READ RECORD (1)B$
1020 L0=ASC(B$(1,1)),L1=1
1030 IF L0=0 THEN L0=DEC(B$(1,3)),L1=3
1100 IF LEN(B$)<L0 THEN READ RECORD (1)C$; B$=B$+C$,C$=""; GOTO 1100
1120 Q$=B$(L1+1,L0-L1)
1130 IF Q$=$271034F2$ OR Q$=$271043F2$ THEN GOTO 9900
1150 W$=LST(Q$); PRINT W$
1154 IF NUM(W$(1,4))<60 THEN GOTO 1160
1155 IF W$(6,3)="REM" THEN GOTO 1280
1160 GOSUB 2000
1280 B$=B$(L0+1)
1290 GOTO 1000
2000 REM "WRITE $$
2020 Q1$=LST(Q$)
2030 IF LEN(Q1$)<80 THEN WRITE (2)Q1$; RETURN 
2040 Q2$=Q1$(80),Q1$=Q1$(1,79)
2050 WRITE (2)Q1$
2060 Q1$=Q1$(1,4)+":"+Q2$,Q2$=""
2070 GOTO 2030
