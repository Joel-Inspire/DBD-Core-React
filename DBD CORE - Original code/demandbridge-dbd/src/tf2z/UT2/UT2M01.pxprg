0010 REM "Copy 061 to Div 20 of 076 <UT2M01>
0020 SETESC 9300; SETERR 9000
0035 REM "3.6 - 10/17/92 - 15.92
0040 REM "Copyright 1992 Computer Software Inc.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,Q0$,Q1$
0100 SETERR 9000
0110 X0$="UT2M01",X1$="Copy 061 to div 20 of 076"
0120 DIM Z0$(80,"-")
0130 K0=20,K1=1
0135 C9=-1
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=MNM('GS')+Z0$+MNM('GE')
0300 REM "IOLISTS
0310 IF X3$(9,3)="076" THEN GOTO 0400
0320 CALL "ZZPROM",".4",X3$,0,"You must be in Company 076 to run this utility!","","",0; GOTO 9950
0400 REM C0$=SOURCE COMPANY, C1$=DEST COMPANY, D0$=DEST DIVISION
0410 C0$="061",C1$="076",D0$="20"
0420 F1$="AR1 AR0 AR9 AS5 FM1 FM3 FM4 FMS FMU FMV FS6 FS9 "
0430 GOSUB 6000
0440 IF Q1$="" THEN CALL "ZZPROM",".Y",X3$,Z,"Proceed?","","",0; ON Z GOTO 0441,9900
0450 DIM Z[NUM(X3$(60,3))]
0500 F$=F1$(1,3),F1$=F1$(5)
0505 IF F$="" THEN GOTO 5000
0510 Z$="01O "+F$+C0$+"  02O "+F$+"..."
0520 GOSUB 9750
0990 READ (Z[1],KEY="",DOM=0991)
1000 REM 
1020 K$=KEY(Z[1],END=2000); READ RECORD (Z[1],KEY=K$)A$
1040 PRINT @(10,10),"File: ",F$," Key: ",K$,
1060 K$(1,2)=D0$,A$(1,2)=D0$
1080 IF F$="AR1" THEN IF POS(" "<>A$(46,10))>0 THEN A$(46,2)=D0$
1100 IF F$="FM1" THEN IF POS(" "<>A$(185,10))>0 THEN A$(185,2)=D0$
1110 IF F$="FM1" THEN IF POS(" "<>A$(132,8))>0 THEN A$(132,2)=D0$
1120 IF F$="FS6" THEN A$(22,2)=D0$
1450 WRITE RECORD (Z[2],KEY=K$)A$
1490 GOTO 1000
2000 REM 
2020 Z$="01C "+F$+C0$+"  02C "+F$+"..."
2040 GOSUB 9750
2060 GOTO 0500
5000 REM "EOJ
5020 CALL "AR2UT8",X3$,X4$,"","*"
5040 CALL "FM2UT2",X3$,X4$,"","*"
5060 CALL "FM2TU4",X3$,X4$,"","*"
5180 IF Q1$="" THEN CALL "ZZPROM",".4",X3$,Z,"Process complete!!","","",0
5190 GOTO 9900
6000 REM "BACKGROUND
6005 PRINT (0,ERR=6016)'SB',
6020 PRINT @(0,4),"This CSI utility program will copy records from company 061 into division 20 of company 76.  The following files will be processed:",'LF',F1$,
6165 PRINT (0,ERR=6166)'SF',
6190 RETURN 
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9055 REM 
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9750 REM 
9760 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 9761,9900
9790 RETURN 
9900 REM "END PROGRAM
