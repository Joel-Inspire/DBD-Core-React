0010 REM "WRITE BATCH FILES RECORD IN ZZPARM <UT2BATCH>
0100 BEGIN 
0110 OPEN (1)"ZZE"
0120 OPEN (13)"ZZPARM"
0200 READ (1,END=5000)A$
0220 PRINT @(0,15),A$(1,6),@(10),A$(7,39),'CL',
0290 IF A$(46,1)<>"1" THEN GOTO 0200
0400 Q$=A$(1,6); IF A$(4,3)="   " THEN A$(4,3)="..."
0420 B$=B$+A$(1,6)+"  "
0440 PRINT "BY BATCH!",
0490 GOTO 0200
5020 B$="BATCH"+B$
5040 WRITE (13,KEY=B$(1,5))B$
5090 GOTO 9900
9960 RUN "ZMENU"
9999 END 
