0010 REM "CREATE *VBU <UT2VBU>
0100 BEGIN 
0110 OPEN (1)"CSYSFL"+FID(0)
0120 OPEN (2)"*VBU"
0130 F$=FID(2)
0140 CLOSE (2); ERASE F$(4,6),ERR=0145; FILE F$
0150 OPEN (2)F$(4,6)
0160 PRINT 'CS',"COPY CSYSFL TO *VBU",'LF',"ENTER DISK NUMBER FROM WHICH TO BACKUP: ",; INPUT (0,ERR=0160)D$:(LEN=1,1)
0165 IF D$<"0" OR D$>"9" THEN END 
0200 K$=KEY(1,END=4000); READ (1)
0250 PRINT K$
0300 WRITE (2,KEY=D$+K$)
0400 GOTO 0200
