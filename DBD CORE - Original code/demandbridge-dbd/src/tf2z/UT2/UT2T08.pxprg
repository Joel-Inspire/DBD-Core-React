1350 IF J>=40001 AND J<=40418 THEN A$(45,2)="01"; GOTO 1500
1360 IF J>=40419 AND J<=40610 THEN A$(45,2)="02"; GOTO 1500
1370 IF J>=40611 AND J<=40760 THEN A$(45,2)="03"; GOTO 1500
