0010 REM 'COPY ALL OF DMPARM TO ZZPARM <UT2DRM>
0035 REM "3.4 - 01/20/91 - 10.34
0040 REM "Copyright 1991 Computer Software Inc.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0100 BEGIN 
0110 OPEN (1)"DMPARM"
0120 OPEN (2)"ZZPARM"
0200 F9$="ZZPARM"
0250 PRINT 'CS',"Copy Company 500 records from DMPARM into ZZPARM? (N= return to normal mode) "; INPUT D9$
0260 IF POS(D9$="YN")=0 THEN GOTO 0250 ELSE IF D9$="Y" THEN GOTO 4000
0300 PRINT 'CS',"This Program will copy all records in DMPARM to ZZPARM"
0400 INPUT "CONTINUE? (CSI response means ZZPARM >> DMPARM)",Q$
0410 IF Q$="CSI" THEN P$="Y"; GOTO 3000
0420 IF Q$<>"Y" THEN GOTO 9999
0440 INPUT "DO YOU WANT SAFE PROMPTING? (Y/N)",P$
0460 IF P$<>"Y" AND P$<>"N" THEN GOTO 0440
1000 IF F9$<>"DMPARM" THEN K$=KEY(1,END=9900) ELSE GOTO 3040
1020 READ RECORD (1,KEY=K$)R$
1100 PRINT K$
1150 IF P$<>"Y" THEN Q$="" ELSE INPUT "TRANSFER? (CR/N)",Q$
1160 IF Q$<>"" THEN PRINT "SKIPPED!"; GOTO 1000
1200 WRITE RECORD (2,KEY=K$,DOM=2000)R$
1300 GOTO 1000
2000 IF P$<>"Y" THEN Q$="" ELSE INPUT "RECORD EXISTS IN "+F9$+"! OVERWRITE IT? (CR/N) ",Q$
2020 IF Q$="" THEN WRITE RECORD (2,KEY=K$)R$; PRINT "OVERWRITTEN!"; GOTO 1000
2040 PRINT "SKIPPED!"
2060 GOTO 1000
3000 REM "COPY FROM ZZPARM TO DMPARM
3020 F9$="DMPARM"
3025 CLOSE (1); CLOSE (2)
3030 OPEN (1)"ZZPARM"; OPEN (2)"DMPARM"
3040 INPUT "ENTER KEY: ",K$
3050 IF CTL=4 OR K$="END" THEN GOTO 9900
3060 FIND (1,KEY=K$,DOM=3040)
3080 GOTO 1020
4000 REM "COPY DMPARM INTO ZZPARM
4020 BEGIN 
4040 OPEN (1)"DMPARM"
4060 OPEN (2)"ZZPARM"
4080 K$=KEY(1,END=9900)
4100 READ RECORD (1,KEY=K$)R$
4110 PRINT K$
4120 WRITE RECORD (2,KEY=K$)R$
4130 GOTO 4080
5000 PRINT "EOJ"
5020 GOTO 9900
9900 PRINT "EOJ"
9999 END 
