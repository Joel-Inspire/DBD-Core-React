5040 GOTO 9900
6000 REM "BACKGROUND
6005 PRINT (0,ERR=6016)'SB',
6020 PRINT @(0,4),"This CSI utility will process the Alternate Commission File AS4 and remove any records PRIOR TO the Fiscal Year and Accounting Period entered",
6040 PRINT @(20,8),"Fiscal Year:",@(14,10),"Accounting Period:",
6165 PRINT (0,ERR=6166)'SF',
6190 RETURN 
