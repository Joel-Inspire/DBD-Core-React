0130 K0=20,K1=1
0135 C9=-1
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=MNM('GS')+Z0$+MNM('GE')
0300 REM "IOLISTS
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01O AS4...  13O ZZPARM  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0600 REM "
0610 GOSUB 6000
0640 CALL "ZZPROM",".Y",X3$,Z,"Proceed?","","",0; ON Z GOTO 0641,9900
0700 DIM A$(6)
0710 CALL "ZZENTR","Z",A{ALL},A$,X4$,X3$,35,8,1,4,C0,"",X$,"","UT2AS400","","",""; IF ABS(C0)>=4 THEN GOTO 9900
0720 CALL "ZZENTR","Z",A{ALL},A$,X4$,X3$,35,10,5,2,C0,"",X$,"","UT2AS402","","",""; IF ABS(C0)>=4 THEN GOTO 9900
0780 CALL "ZZPROM",".Y",X3$,Z,"Proceed?","","",0; ON Z GOTO 0781,9900
0990 PRINT @(25,12),"Commission Record",
1000 REM 
1010 K$=KEY(Z[1],END=5000)
1025 IF K$(1,6)>=A$ THEN GOTO 5000
1100 REMOVE (Z[1],KEY=K$)
1200 C=C+1
1240 PRINT @(20,13),C:"#####0","-",K$(1,4),"-",K$(5,2),"-",K$(7,4),"-",K$(11,8),"-",K$(19,3),
1600 GOTO 1000
5000 REM "EOJ
5010 X$=STR(C:"000000")+" records removed"
