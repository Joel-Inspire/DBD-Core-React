0050 REM + Begin Program Notes
0052 REM "The purpose of this program is to read through the ZZS file
0053 REM "and check to see if the records for FM2RBA reports are 
0054 REM "padded for the pick priority. If the records are not, then
0055 REM "the program will pad the records and re-write them.
0099 REM + End Program Notes
0100 REM + Begin Program Initialization
0110 CLEAR 
0130 ENTER X3$,X4$,Q0$,Q1$,ERR=*NEXT
0140 SETERR ERROR_PROCESSING
0150 SETESC ESCAPE_PROCESSING
0200 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,X5$,X0,X1,0 ! call ZZCOMP
0299 REM + End Program Initialization
0300 REM + Begin String and Variable Declaration
0305 X0$=PGN
0310 X1$="ZZS - FM2RBA Conversion"
0320 DIM Z[255]
0330 RPT_NAME$="FM2RBA" ! set the partial key
0340 PICK_PRIOR_GRP$=PAD(" ",60) ! fill the string with spaces
0350 ZZS_REC_LEN=275 ! the new rec len
0360 DIM W[5]
0499 REM + End String and Variable Declaration
0500 REM + Begin Open Files
0510 Z$=""
0520 Z$+="01L ZZS...  " ! ZZS File
0580 GOSUB OPEN_FILES ! open the files
0590 ZZS_LFN=Z[1] ! set the ZZS lfn
0799 REM + End Open Files
0800 REM + Begin IOLISTs
0805 IOLIST ZZS$ ! ZZS record
0810 IOLIST R0$,W$,W[0],W[1],W[2],W[3],W[4] ! ZZS record
0999 REM + End IOLISTs
1000 REM + Begin Program Main Routine
1010 ZZS_KEY$=RPT_NAME$
1012 READ (ZZS_LFN,KEY=ZZS_KEY$,ERR=ERROR_PROCESSING,DOM=*NEXT) ! read ZZS
1020 REPEAT ! begin loop
1025 ZZS_KEY$=KEY(ZZS_LFN) ! get the key
1030 READ (ZZS_LFN,KEY=ZZS_KEY$,ERR=ERROR_PROCESSING,DOM=*NEXT)IOL=0805,IOL=0810 ! read ZZS
1040 IF ZZS_KEY$(1,6)=RPT_NAME$ THEN {! if a valid type found
1050 IF LEN(ZZS$)<>ZZS_REC_LEN THEN {! if the key is the old format
1060 ZZS$+=PAD(" ",ZZS_REC_LEN-LEN(ZZS$)) ! append the prick prior group
1070 WRITE (ZZS_LFN,KEY=ZZS_KEY$,ERR=ERROR_PROCESSING)IOL=0805,IOL=0810 ! write the record
1150  }
1160  }
1170 UNTIL ZZS_KEY$(1,6)<>RPT_NAME$ ! loop until no more type 'L' records
1295 GOTO END_PROGRAM ! end
9000 ERROR_PROCESSING:
9001 REM "ERROR PROCESSING Routine
9010 Y5=ERR,Y6=TCB(5); IF Y5=68 OR Y5=69 THEN GOTO 9500
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,EXIT_PROGRAM,9070,9090
9055 REM 
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR ERROR_PROCESSING; RETRY 
9090 SETERR 0000; RETRY 
9100 TRANSFER_CONTROL:
9101 REM " TRANSFER CONTROL
9120 ON Y8 GOTO END_PROGRAM,0990,9920
9180 GOTO 0990
9190 GOTO EXIT_PROGRAM
9300 ESCAPE_PROCESSING:
9301 REM "Escape Processing Routine
9305 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)<>"1" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR ERROR_PROCESSING; SETESC ESCAPE_PROCESSING; RETURN 
9500 CTRL_LOGIC:
9501 REM "Control Logic
9510 SETERR ERROR_PROCESSING; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9740 RETURN 
9750 OPEN_FILES:
9751 REM "Open Files Routine
9760 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1
9790 RETURN 
9800 EXIT_PROGRAM:
9801 REM "EXIT PROGRAM
9900 END_PROGRAM:
9901 REM "END PROGRAM
9905 Y8=2
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9930 SETERR 9940; Q1$=A1$; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
