0010 BEGIN 
0100 OPEN (1)"AP4015"
0120 OPEN (2)"AP4NEW"
0130 OPEN (3)"AP6015"
0500 M1$="00000000"
0510 N=2500
1000 K$=KEY(3,END=9900)
