0320 CLOSE (1); OPEN (1,ERR=0290)F$
1000 REM "
1010 K$=KEY(1,END=2000)
1015 PRINT @(0),K$,
1020 READ RECORD (1,KEY=K$)Q0$
1100 REM "
1110 Q1$=""
1120 CALL "ZZREAD",1,K$,Q1$,S8
1200 REM "
1210 IF Q1$<>Q0$ THEN PRINT " FAULT READ"
1390 GOTO 1000
