0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,Q0$,Q1$
0100 SETERR 9000
0110 X0$="EC3UT9",X1$="WebEC Item Parameter Category Utility",K9$="",K9=0
0120 DIM Z0$(80,"-")
0130 K0=9,K1=1
0135 C9=-1,V9=-2
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0295 DIM Z0$(80,X3$(235,1)); Z0$=ESC+"BG"+Z0$+ESC+"EG"
0300 REM "IOLISTS
0310 IOLIST A$
0320 IOLIST AR1$
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="02O AR1...  03O ECG...  04O ECR...  13O ZZPARM "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0529 K0$="[128]Item Parameter Update File"
0530 CALL "ZZ2SRT",X3$,X4$,Z[13],-100,20,K0$,K1,"NN"
0535 Z$="01O "+K0$+" "
0540 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0541,9900
0600 REM "
0620 GOSUB 6000
0700 REM "Check for existing entries
0710 K$=KEY(Z[1],END=0800)
0720 CALL "ZZPROM","X0AP2CBA",X3$,Z,"Entries exist in the Data Enry File.","","",0; ON Z GOTO 0800,0721,9900
0730 CALL "ZZINIT",STR(Z[1]:"00")
0800 REM 
0900 REM "1,10=customer code, 11,10=form code, 21,10=from category, 31,10=to category
1000 REM "
1010 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,X5$,X0,X1,X2
1030 DIM B$(100),B0$(100),C$(100),C0$(100),D$(100),D0$(100),D8$(40),D9$(40),AR1$(700),ECG1$(106),ECG2$(106)
1035 DIM A$(41)
1040 IF V9<>-1 THEN GOSUB 6900
1045 A$(1,0+K9)=K9$+A1$
1060 PRINT (0,ERR=1070)'CF',; GOSUB 6450; GOTO 1100
1070 GOSUB 6000; GOSUB 6450
1100 REM "KEY SEQ
1110 X0=-1,C0=0,X1=0,C9=0,Z9=0
1120 C0=ABS(C0),X$=""; IF C0=2 AND X0>0 THEN X0=X0-1 ELSE IF C0<>2 THEN X0=X0+1; IF C0=4 THEN GOTO 5000
1140 ON X0 GOTO 1200,1250,1500
1200 REM "Customer code
1205 X$="Use F1-F3 report option to obtain printout prior to updating"
1210 CALL "ZZENTR","AUZX",A{ALL},A$,X4$,X3$,20,3,1,10,C0,"A/R","{1"+X$,"","EC3UT900","AR1","",""; IF ABS(C0)>4 THEN GOSUB 6400; ON C0 GOTO 1210,1211
1215 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1210,6800,5000,6970
1220 FIND (Z[2],KEY=A$(1,10),DOM=1200)IOL=0320; GOSUB 6450
1240 GOTO 1120
1250 REM "From Catalog Category
1255 X$="Enter the pattern match (?* wildcards available)"
1260 CALL "ZZENTR","SUZX",A{ALL},A$,X4$,X3$,20,4,11,10,C0,"","{1"+X$,A$(1,10),"AR2UTZ02","","",""; IF ABS(C0)>4 THEN GOSUB 6400
1265 IF C0<0 THEN ON INT(ABS(C0)-2) GOTO 1266,6800,5000,6970
1290 GOTO 1120
1500 REM "
1505 Z9=-1
1510 A1$=A$(1,20)
1520 READ (Z[1],KEY=A1$,DOM=1600)IOL=0310
1525 Z9=0
1530 GOSUB 6600
1590 GOTO 1700
1600 REM "
1620 CALL "ZZPROM","2",X3$,Z,"","","",0; ON Z GOTO 1650,1630,6975
1630 GOTO 1000
1650 A$(7,3)=A$(1,3); GOSUB 6220
1690 GOTO 2000
1700 REM "
1790 GOSUB 6200
1800 REM "
1820 CALL "ZZPROM",STR(6+C9),X3$,Z,"","","V",X0; ON Z GOTO 1850,6500,1630,6700,6800
1830 GOTO 2000
1850 REM "MODIFY
1855 X0=X0-1; IF Z9=1 THEN GOTO 1880 ELSE IF Z9=0 THEN Z9=1
1860 EXTRACT (Z[1],KEY=A1$,DOM=1870)IOL=0310
1865 C9=1
1870 GOSUB 6200
1890 IF Z<0 THEN GOTO 2035 ELSE GOTO 2000
2000 REM "DATA SEQ
2010 C0=0,X0=-1,X1=1
2020 C9=1,X$="",C0=ABS(C0),X0=X0+1; IF C0=2 THEN IF X0>0 THEN X0=X0-2
2025 IF C0>3 OR X0<0 THEN GOTO 1800
2040 ON X0 GOTO 2100,2150,1800
2100 REM "To Category
2110 CALL "ZZENTR","SUZX",A{ALL},A$,X4$,X3$,20,7,21,10,C0,"","{3"+X$,"","EC3UT904","ECG","",""; IF ABS(C0)>4 THEN GOSUB 6400; ON C0 GOTO 2160,2161
2115 IF STP(A$(21,10),3," ")="" THEN DIM ECG1$(106) ELSE FIND (Z[3],KEY=A$(21,10),DOM=2110)ECG1$
2120 PRINT @(32,7),ECG1$(11,30),
2140 GOTO 2020
2150 REM "To Catalog Category
2160 CALL "ZZENTR","SUZX",A{ALL},A$,X4$,X3$,20,8,31,10,C0,"","{3"+X$,"","EC3UT906","ECG","",""; IF ABS(C0)>4 THEN GOSUB 6400; ON C0 GOTO 2160,2161
2162 DIM ECGTOREC$(200); IF POS(" "<>A$(21,30))<>0 THEN ECGTOREC$(11,30)="NEW category" END_IF ; FIND (Z[3],KEY=A$(31,10),DOM=2163)ECGTOREC$
2163 ECG2$=ECGTOREC$
2164 PRINT @(32,8),ECGTOREC$(11,30),
2170 IF C0=2 THEN GOTO 2020
2190 GOTO 2020
2200 REM "copy/delete existing category
2202 X$="C=leaves in existing category  M=moves to new category"
2210 CALL "ZZENTR","SUZX",A{ALL},A$,X4$,X3$,20,8,41,1,C0,"","{3"+X$,A$(1,10),"AR2UTZ06","ECH","",""; IF ABS(C0)>4 THEN GOSUB 6400; ON C0 GOTO 2160,2161
2212 IF POS(A$(41,1)="CM")=0 THEN GOTO 2200
2240 GOTO 2020
5000 REM "End of entry
5020 CALL "ZZPROM","9",X3$,Z,"Update Now?","","",0; IF Z=1 THEN GOTO 9900
5090 GOSUB 7500
5095 CALL "ZZINIT",STR(Z[1]:"00")
5100 IF Q1$="" THEN CALL "ZZPROM",".4",X3$,Z,"Process complete!!","","",0
5110 RUN "EC3UT3"
6000 REM "BACKGROUND
6005 PRINT (0,ERR=6016)'SB',
6060 PRINT @(1,3),"    Customer code: ",@(1,4),"Item code pattern:",@(0,5),Z0$,
6065 PRINT @(0,7),"From category code:",@(2,8),"To Category code:",
6070 PRINT @(5,10),"To add a category to the list in the form parameters",@(5,11),"put new category in To Category, leave From Category blank. New category",@(5,12),"will be added if not already there and there is room.",
6072 PRINT @(5,14),"To delete a category, leave To Category code blank,",@(5,15),"and put category to delete in the From Category code.",
6074 PRINT @(5,17),"To change from one category to another, put the old",@(5,18),"category in From Category code, new category in To Category code.",
6165 PRINT (0,ERR=6166)'SF',
6190 RETURN 
6200 REM "DISPLAY DATA
6215 GOSUB 6450
6220 PRINT @(20,7),A$(21,10),@(20,8),A$(31,10),
6230 PRINT @(32,7),ECG1$(11,30),@(32,8),ECG2$(11,30),
6390 RETURN 
6400 REM "WHOLE SCREEN
6405 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,X5$,X0,X1,0
6420 GOSUB 6000
6430 IF C9>=0 THEN GOSUB 6200 ELSE GOSUB 6450
6440 IF ABS(C0)>4 THEN C0=ABS(C0)-5
6445 RETURN 
6450 REM "DISPLAY KEYS
6455 IF C9<0 THEN GOTO 6445
6459 CALL "ZZDISP","A",A$(1,10),"A/R",X3$,"","",20,3+V0,X4$
6460 PRINT @(20,4),A$(11,10),@(32,3),AR1$(11,30),
6490 RETURN 
6500 REM "DELETE
6510 REMOVE (Z[1],KEY=A1$,DOM=6511)
6590 GOTO 1630
6600 REM "ALT KEY DATA READS
6605 READ (Z[3],KEY=A$(21,10),DOM=6606)ECG1$
6610 READ (Z[3],KEY=A$(31,10),DOM=6611)ECG2$
6690 RETURN 
6700 REM "ADD ENTRY
6705 IF Z9=0 THEN GOTO 6790
6710 WRITE (Z[1],KEY=A1$)IOL=0310
6790 GOTO 1630
6800 REM "NEXT ENTRY IN FILE
6805 ON 10*FPT(C0)+2 GOTO 6850,6806
6810 A1$=KEY(Z[1],END=6895)
6820 A$(1,LEN(A1$))=A1$
6825 IF A1$(1,LEN(K9$))<>K9$ THEN GOTO 1140
6830 GOSUB 6600
6835 GOSUB 6450
6840 GOTO 1515
6850 REM PRIOR ENTRY
6855 CALL "ZZKEYP",ERR=6856,Z[1],A1$; GOTO 6820
6860 CALL "ZZKEYL",ERR=6861,Z[1],A1$; GOTO 6820
6870 GOTO 6895
6890 GOTO 1515
6895 PRINT 'RB',; READ (Z[1],KEY=K9$,DOM=1140); GOTO 1140
6900 REM "GET DEFAULTS
6902 V9$="2203  2205  "
6910 IF V9>=0 THEN READ (Z[13],IND=V9)V9$,IOL=0310; GOTO 6930 ELSE V9=-1
6920 EXTRACT (Z[13],KEY=X0$+"DFLT",DOM=6921)V9$,IOL=0310; V9=IND(Z[13]); READ (Z[13])
6930 RETURN 
6950 REM "WRITE DEFAULT
6955 WRITE (Z[13],KEY=X0$+"DFLT")V9$,IOL=0310
6960 GOSUB 6920; GOTO 1630
6970 REM "ENTRY DEFAULTS
6975 PRINT 'CF'; V9=-2,Q$=""
6980 FOR X=1 TO LEN(V9$) STEP 6
6982 PRINT @(NUM(V9$(X,2)),NUM(V9$(X+2,2))),; IF V9=-2 THEN PRINT V9$(X+4,1), ELSE READ RECORD (0,SIZ=1)Q$; IF POS(Q$="1234Z")=0 THEN PRINT 'RB'; GOTO 6982
6990 IF Q$="Z" THEN EXITTO 6950 ELSE V9$(X+4,LEN(Q$))=Q$; NEXT X
6995 V9=0; GOTO 6980
7500 REM "Update
7510 READ (Z[1],KEY="",DOM=7511)
7520 A1$=KEY(Z[1],END=7600); READ (Z[1],KEY=A1$)A$
7525 IF STP(A$(31,10),3," ")="" THEN GOTO 7526 ELSE FIND (Z[3],KEY=A$(31,10),DOM=7520); REM "if category not on file then skip
7526 IF STP(A$(21,10),3," ")="" AND STP(A$(31,10),3," ")="" THEN GOTO 7520 ELSE IF STP(A$(21,10),3," ")="" THEN MODE$="ADD" ELSE IF STP(A$(31,10),3," ")="" THEN MODE$="DELETE" ELSE MODE$="CHANGE"
7527 CUSTOMER$=A1$(1,10),PATTERN$=STP(A1$(11,10),2)
7528 READ (Z[4],KEY=CUSTOMER$,DOM=7529)
7530 REM "process parameters
7532 ECRKEY$=KEY(Z[4],END=7520); IF ECRKEY$(1,LEN(CUSTOMER$))<>CUSTOMER$ THEN GOTO 7520 ELSE READ RECORD (Z[4],KEY=ECRKEY$)ECR$
7534 IF STP(ECR$(11,10),1) LIKE PATTERN$ THEN GOTO 7535 ELSE GOTO 7532
7540 CAT_LIST$=ECR$(21,10)+ECR$(193,40)
7545 IF MODE$="ADD" THEN GOTO 7550 ELSE IF MODE$="DELETE" THEN GOTO 7560 ELSE GOTO 7570
7550 REM "ADD to category (A$(31,10)) to list if not there and there is space
7552 IF POS(A$(31,10)=CAT_LIST$,10)=0 THEN TMP=POS(DIM(10)=CAT_LIST$,10); IF TMP<>0 THEN CAT_LIST$(TMP,10)=A$(31,10)
7558 GOTO 7580
7560 REM "DELETE from category (A$(21,10)) if there
7562 TMP=POS(A$(21,10)=CAT_LIST$,10); IF TMP=0 THEN GOTO 7568 ELSE CAT_LIST$(TMP,10)=""
7563 REM "Move up categories to fill blanks
7564 FOR I=11 TO 41 STEP 10
7565 IF STP(CAT_LIST$(I-10,10),3," ")="" THEN CAT_LIST$(I-10,10)=CAT_LIST$(I,10),CAT_LIST$(I,10)=""
7566 NEXT I
7568 GOTO 7580
7570 REM "IF from category (A$(21,10)) is present, then replace with To category (A$(31,10))
7572 TMP=POS(A$(21,10)=CAT_LIST$,10); IF TMP<>0 THEN CAT_LIST$(TMP,10)=A$(31,10)
7578 GOTO 7580
7580 REM WRITE OUT RECORD AND CONTINUE
7582 ECR$(21,10)=CAT_LIST$(1,10),ECR$(193,40)=CAT_LIST$(11,40)
7585 WRITE RECORD (Z[4],KEY=ECRKEY$)ECR$
7587 GOTO 7532
7600 RETURN 
7990 GOTO 9900
8100 REM "GOSUB here, once, at the beginning, after the background is set and before the Proceed? question. Set T to total # to do (by calling ZZINFO) and T0 to the reporting interval (report every T0 number of records) this is typically 2% of total; This also prints a message (@8115),so adjust accordingly
8110 CALL "ZZINFO",Z[F],T9,X3$,T,T0,K,B9,D,S0,S1,F$
8115 PRINT @(0,7),"There are "+STR(T)+" records to process"
8129 REM "Set T0, we make sure T0 is > 1, because later on we MOD and look for avalue of 1. IF T0 is 1, then nothing would get reported. We look for a result of 1 because this causes the first record to automatically start the reporting instead of waiting until the T0'th record to get the first report
8130 T0=INT(T*.02); IF T0<=1 THEN T0=2
8145 RETURN 
8150 REM "Call this each time to update the bar graph. Display horiz. bar graph given total #, T and current #, C, and time counter T1 (T1 is init'ed here and used here, just don't use it somewhere else)
8155 CALL "ZZBARG",X3$,"HG",19,10,50,T1,T,C
8195 RETURN 
