4540 FIND (_FS9,KEY=ORDER_DIV$+ORDER_NUM$+ITEM_CODE$+SEQ_NUM$,DOM=*NEXT)FS9$; ITEM_DESC$=FS9$(41,40); GOTO 3068 ! SSP217063 jdf
4550 FS9_NUM=FS9_NUM+1 ! SSP217063 jdf
4560 FS9_NUM$=STR(FS9_NUM) ! SSP217063 jdf
4570 GOTO 4530 ! SSP217063 jdf
4580 ITEM_DESC$=DIM(40) ! SSP217063 jdf
4590 RETURN ! SSP217063 jdf
