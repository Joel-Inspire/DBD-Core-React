17160 IF SP<>0 THEN {
17165 TF_NOTE$=MID(ON$,1,SP-1); ON$=MID(ON$,SP+1)
17166 IF LEN(TF_NOTE$)>50 THEN {
17167 FOR XX=51 TO 1 STEP -1; IF TF_NOTE$(XX,1)=" " THEN EXITTO 17169
17168 NEXT XX
17169 BNOTE$=TF_NOTE$(XX),TF_NOTE$=TF_NOTE$(1,XX-1),ON$=BNOTE$+ON$+SEP,SP=1
17170  }
17172 INDEX=INDEX+1; NOTEPAD_LINE_NAME$="NOTEPAD_LINE_"+STR(INDEX)
17175 VIA NOTEPAD_LINE_NAME$=TF_NOTE$
17180  }
17185 UNTIL INDEX=10 OR SP=0
17190 RETURN 
