1481 NEXT_FOLDER=FLDR.QSGDAA.1.CTL
1490 RETURN ! SSP219064 jdf
1600 ! 1600 - Delete
1610 DELETE_REC:
1620 READ DATA FROM REC(IOL(_FIL_NO:KEY,ERR=*NEXT)) TO _KEY$
1630 _R_KEY$=_KEY$; TRANSLATE _R_KEY$," ",$00$
1640 MSGBOX MSG("DELETE_REC"),MSG("CONFIRM"),"?,YESNO",_X$; IF _X$<>"YES" THEN RETURN ! SSP 189449
1660 REMOVE (_FIL_NO,KEY=_KEY$,ERR=*NEXT)
1665 QUOTE$=_KEY$; GOSUB DELETE_RECORDS ! SSP 193151
1680 IF _ENABLE_FLG THEN _KCNT=1,_ENABLE_FLG=1; GOSUB ENABLE_GROUPS
1690 CHANGE_FLG=0; _CLR_FLG$="R"; GOSUB CLEAR_FIELDS ! SSP 189449
1700 NEXT_ID=_FIRST_KEY
1710 EXIT 
