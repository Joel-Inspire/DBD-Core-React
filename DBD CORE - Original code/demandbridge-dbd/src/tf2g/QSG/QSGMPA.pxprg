1040 RFQ$(1,KEY_SIZE)=PARM_KEY$
1050 READ (_ZZPARM,KEY=PARM_KEY$,DOM=*NEXT)IOL=RFQ_IOL
1060 REFRESH_FLG=1
1070 RETURN 
1080 ! 
1200 ! ^1200,10 Write out parameters
1210 WRITE_REC:
1220 WRITE (_ZZPARM,KEY=PARM_KEY$)IOL=RFQ_IOL
1230 CMD_STR$="E"
1240 RETURN 
1250 ! 
9900 ! ^9900,10 - Wrap up
9910 WRAP_UP:
9920 CLOSE (_ZZPARM)
9930 RETURN 
9999 END 
