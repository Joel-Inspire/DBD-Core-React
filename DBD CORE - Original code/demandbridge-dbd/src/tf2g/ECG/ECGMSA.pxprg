12010 CHECK_REQD_FLDS:
12020 _W_FLG=1
12999 RETURN 
13000 ! 13000 - Clear the record fields
13010 CLEAR_FIELDS:
13020 IF _CLR_FLG$="R" THEN SERVER_ID$=""
13030 SERVER_DESC$=""
13040 SERVER_DIRECTORY$=""
13050 ARGUMENTS$=""
13060 FILE_MASK$=""
13070 LOG_FILE$=""
13080 BASE_PAGE_REF$=""
13090 CGI_COMMAND$=""
13100 KILL_AFTER_SECS$=""
