15160 IF _ORIG.PROD_CAT_NOT_USED$=_CUR.PROD_CAT_NOT_USED$ OR PROD_CAT_NOT_USED$=_CUR.PROD_CAT_NOT_USED$ THEN GOTO *NEXT ELSE IF _ORIG.PROD_CAT_NOT_USED$=PROD_CAT_NOT_USED$ THEN _CHG2$=_CHG2$+SEP+MSG("PRD_CT_N_U")+" = "+_CUR.PROD_CAT_NOT_USED$,_CHG_VAR$(14,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("PRD_CT_N_U")+" = "+_CUR.PROD_CAT_NOT_USED$
