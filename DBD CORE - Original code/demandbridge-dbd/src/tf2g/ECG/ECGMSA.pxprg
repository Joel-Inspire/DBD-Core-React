14000 ! 14000 - Enable/Disable groups - _ENABLE_FLG: 0=don't!, >1=corresponding key segment on, others off, -1=buttons and non-key fields on
14010 ENABLE_GROUPS:
14020 IF _ENABLE_FLG=0 THEN GOTO *RETURN
14970 IF _ENABLE_FLG>0 THEN CALL "*wingrp;Disable",FIELDS.GRP$; CALL "*wingrp;Disable",BUTTONS.GRP$
14980 IF _ENABLE_FLG<0 THEN CALL "*wingrp;Enable",FIELDS.GRP$; CALL "*wingrp;Enable",BUTTONS.GRP$
14990 RETURN 
15000 ! 15000 - Determine the changes that have been made before writing
15010 DETERMINE_CHGS:
15020 DIM _CHG_VAR$(19,"N") ! SSP242458
15030 IF _ORIG.SERVER_ID$=_CUR.SERVER_ID$ OR SERVER_ID$=_CUR.SERVER_ID$ THEN GOTO *NEXT ELSE IF _ORIG.SERVER_ID$=SERVER_ID$ THEN _CHG2$=_CHG2$+SEP+MSG("SERVER_ID")+" = "+_CUR.SERVER_ID$,_CHG_VAR$(1,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("SERVER_ID")+" = "+_CUR.SERVER_ID$
15040 IF _ORIG.SERVER_DESC$=_CUR.SERVER_DESC$ OR SERVER_DESC$=_CUR.SERVER_DESC$ THEN GOTO *NEXT ELSE IF _ORIG.SERVER_DESC$=SERVER_DESC$ THEN _CHG2$=_CHG2$+SEP+MSG("SERV_D")+" = "+_CUR.SERVER_DESC$,_CHG_VAR$(2,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("SERV_D")+" = "+_CUR.SERVER_DESC$
15050 IF _ORIG.SERVER_DIRECTORY$=_CUR.SERVER_DIRECTORY$ OR SERVER_DIRECTORY$=_CUR.SERVER_DIRECTORY$ THEN GOTO *NEXT ELSE IF _ORIG.SERVER_DIRECTORY$=SERVER_DIRECTORY$ THEN _CHG2$=_CHG2$+SEP+MSG("SERV_DIR")+" = "+_CUR.SERVER_DIRECTORY$,_CHG_VAR$(3,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("SERV_DIR")+" = "+_CUR.SERVER_DIRECTORY$
15060 IF _ORIG.ARGUMENTS$=_CUR.ARGUMENTS$ OR ARGUMENTS$=_CUR.ARGUMENTS$ THEN GOTO *NEXT ELSE IF _ORIG.ARGUMENTS$=ARGUMENTS$ THEN _CHG2$=_CHG2$+SEP+MSG("ARGUMENT")+" = "+_CUR.ARGUMENTS$,_CHG_VAR$(4,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("ARGUMENT")+" = "+_CUR.ARGUMENTS$
15070 IF _ORIG.FILE_MASK$=_CUR.FILE_MASK$ OR FILE_MASK$=_CUR.FILE_MASK$ THEN GOTO *NEXT ELSE IF _ORIG.FILE_MASK$=FILE_MASK$ THEN _CHG2$=_CHG2$+SEP+MSG("FILE_MASK")+" = "+_CUR.FILE_MASK$,_CHG_VAR$(5,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("FILE_MASK")+" = "+_CUR.FILE_MASK$
15080 IF _ORIG.LOG_FILE$=_CUR.LOG_FILE$ OR LOG_FILE$=_CUR.LOG_FILE$ THEN GOTO *NEXT ELSE IF _ORIG.LOG_FILE$=LOG_FILE$ THEN _CHG2$=_CHG2$+SEP+MSG("LOG_FILE")+" = "+_CUR.LOG_FILE$,_CHG_VAR$(6,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("LOG_FILE")+" = "+_CUR.LOG_FILE$
15090 IF _ORIG.BASE_PAGE_REF$=_CUR.BASE_PAGE_REF$ OR BASE_PAGE_REF$=_CUR.BASE_PAGE_REF$ THEN GOTO *NEXT ELSE IF _ORIG.BASE_PAGE_REF$=BASE_PAGE_REF$ THEN _CHG2$=_CHG2$+SEP+MSG("BASE_P_R")+" = "+_CUR.BASE_PAGE_REF$,_CHG_VAR$(7,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("BASE_P_R")+" = "+_CUR.BASE_PAGE_REF$
15100 IF _ORIG.CGI_COMMAND$=_CUR.CGI_COMMAND$ OR CGI_COMMAND$=_CUR.CGI_COMMAND$ THEN GOTO *NEXT ELSE IF _ORIG.CGI_COMMAND$=CGI_COMMAND$ THEN _CHG2$=_CHG2$+SEP+MSG("CGI_C")+" = "+_CUR.CGI_COMMAND$,_CHG_VAR$(8,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("CGI_C")+" = "+_CUR.CGI_COMMAND$
15110 IF _ORIG.KILL_AFTER_SECS$=_CUR.KILL_AFTER_SECS$ OR KILL_AFTER_SECS$=_CUR.KILL_AFTER_SECS$ THEN GOTO *NEXT ELSE IF _ORIG.KILL_AFTER_SECS$=KILL_AFTER_SECS$ THEN _CHG2$=_CHG2$+SEP+MSG("KILL_A_S")+" = "+_CUR.KILL_AFTER_SECS$,_CHG_VAR$(9,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("KILL_A_S")+" = "+_CUR.KILL_AFTER_SECS$
15120 IF _ORIG.DEF_STAT_TEMPLATE$=_CUR.DEF_STAT_TEMPLATE$ OR DEF_STAT_TEMPLATE$=_CUR.DEF_STAT_TEMPLATE$ THEN GOTO *NEXT ELSE IF _ORIG.DEF_STAT_TEMPLATE$=DEF_STAT_TEMPLATE$ THEN _CHG2$=_CHG2$+SEP+MSG("DEF_S_T")+" = "+_CUR.DEF_STAT_TEMPLATE$,_CHG_VAR$(10,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("DEF_S_T")+" = "+_CUR.DEF_STAT_TEMPLATE$
15130 IF _ORIG.TERMINAL_ID$=_CUR.TERMINAL_ID$ OR TERMINAL_ID$=_CUR.TERMINAL_ID$ THEN GOTO *NEXT ELSE IF _ORIG.TERMINAL_ID$=TERMINAL_ID$ THEN _CHG2$=_CHG2$+SEP+MSG("TERM_I")+" = "+_CUR.TERMINAL_ID$,_CHG_VAR$(11,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("TERM_I")+" = "+_CUR.TERMINAL_ID$
15140 IF _ORIG.OPTIONS$=_CUR.OPTIONS$ OR OPTIONS$=_CUR.OPTIONS$ THEN GOTO *NEXT ELSE IF _ORIG.OPTIONS$=OPTIONS$ THEN _CHG2$=_CHG2$+SEP+MSG("OPTIONS")+" = "+_CUR.OPTIONS$,_CHG_VAR$(12,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("OPTIONS")+" = "+_CUR.OPTIONS$
