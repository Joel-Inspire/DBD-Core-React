9280 _MSG_UPDATE$=MSG("UPDATE")
9290 _MSG_UPD_OTHER1$=MSG("UPD_OTHER1")
9300 _MSG_UPD_OTHER2$=MSG("UPD_OTHER2")
9310 _MSG_UPD_SAME$=MSG("UPD_SAME")
9320 _MSG_WARNING$=MSG("WARNING")
9330 _MSG_CANNOT_WRITE$=MSG("CANT_WRITE")
9340 _MSG_DUP_UNIQUE$=MSG("DUP_UNIQUE")
9350 RETURN 
10000 ! 10000 - Convert numeric values to string
10010 NUM_TO_STR:
10020 LOGIN_DATE_TIME_SQL$=STR(LOGIN_DATE_TIME_SQL)
10030 LAST_ACTIVITY_SQL$=STR(LAST_ACTIVITY_SQL)
10040 LOGOFF_SQL$=STR(LOGOFF_SQL)
10999 RETURN 
11000 ! 11000 - Convert numeric strings to number
11010 STR_TO_NUM:
11020 _X$="LOGIN_DATE_TIME_SQL",_X=LOGIN_DATE_TIME_SQL.CTL,LOGIN_DATE_TIME_SQL=NUM(LOGIN_DATE_TIME_SQL$,ERR=NON_NUMERIC)
11030 _X$="LAST_ACTIVITY_SQL",_X=LAST_ACTIVITY_SQL.CTL,LAST_ACTIVITY_SQL=NUM(LAST_ACTIVITY_SQL$,ERR=NON_NUMERIC)
11040 _X$="LOGOFF_SQL",_X=LOGOFF_SQL.CTL,LOGOFF_SQL=NUM(LOGOFF_SQL$,ERR=NON_NUMERIC)
11970 RETURN 
11980 NON_NUMERIC: MSGBOX _MSG_NON_NUMER$+_X$,_ERROR$,"!"
11990 NEXT_ID=_X
11999 EXIT 
12000 ! 12000 - Check if the required fields have data
12010 CHECK_REQD_FLDS:
12020 _W_FLG=1
12999 RETURN 
