14000 ! 14000 - Enable/Disable groups - _ENABLE_FLG: 0=don't!, >1=corresponding key segment on, others off, -1=buttons and non-key fields on
14010 ENABLE_GROUPS:
14020 IF _ENABLE_FLG=0 THEN GOTO *RETURN
14970 IF _ENABLE_FLG>0 THEN CALL "*wingrp;Disable",FIELDS.GRP$; CALL "*wingrp;Disable",BUTTONS.GRP$
14980 IF _ENABLE_FLG<0 THEN CALL "*wingrp;Enable",FIELDS.GRP$; CALL "*wingrp;Enable",BUTTONS.GRP$
14990 RETURN 
15000 ! 15000 - Determine the changes that have been made before writing
15010 DETERMINE_CHGS:
15020 DIM _CHG_VAR$(91,"N")
15030 IF _ORIG.SESSION_ID$=_CUR.SESSION_ID$ OR SESSION_ID$=_CUR.SESSION_ID$ THEN GOTO *NEXT ELSE IF _ORIG.SESSION_ID$=SESSION_ID$ THEN _CHG2$=_CHG2$+SEP+MSG("SESSION_ID")+" = "+_CUR.SESSION_ID$,_CHG_VAR$(1,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("SESSION_ID")+" = "+_CUR.SESSION_ID$
15040 IF _ORIG.BUYER_ID$=_CUR.BUYER_ID$ OR BUYER_ID$=_CUR.BUYER_ID$ THEN GOTO *NEXT ELSE IF _ORIG.BUYER_ID$=BUYER_ID$ THEN _CHG2$=_CHG2$+SEP+MSG("BUYER_ID")+" = "+_CUR.BUYER_ID$,_CHG_VAR$(2,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("BUYER_ID")+" = "+_CUR.BUYER_ID$
15050 IF _ORIG.BUYER_CUSTOMER_NAME$=_CUR.BUYER_CUSTOMER_NAME$ OR BUYER_CUSTOMER_NAME$=_CUR.BUYER_CUSTOMER_NAME$ THEN GOTO *NEXT ELSE IF _ORIG.BUYER_CUSTOMER_NAME$=BUYER_CUSTOMER_NAME$ THEN _CHG2$=_CHG2$+SEP+MSG("BUYE_C_N")+" = "+_CUR.BUYER_CUSTOMER_NAME$,_CHG_VAR$(3,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("BUYE_C_N")+" = "+_CUR.BUYER_CUSTOMER_NAME$
15060 IF _ORIG.BUYER_CUST_LOCT_NAME$=_CUR.BUYER_CUST_LOCT_NAME$ OR BUYER_CUST_LOCT_NAME$=_CUR.BUYER_CUST_LOCT_NAME$ THEN GOTO *NEXT ELSE IF _ORIG.BUYER_CUST_LOCT_NAME$=BUYER_CUST_LOCT_NAME$ THEN _CHG2$=_CHG2$+SEP+MSG("BUYE_C_L_N")+" = "+_CUR.BUYER_CUST_LOCT_NAME$,_CHG_VAR$(4,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("BUYE_C_L_N")+" = "+_CUR.BUYER_CUST_LOCT_NAME$
