17000 LOAD_DATE:
17005 CO$=":"
17007 PRECISION 7
17010 FOR AA=1 TO 3
17020 EXECUTE "XX$=D"+STR(AA)+"$,TMP"+STR(AA)+"$=mid(XX$,5,2)+mid(XX$,7,2)+mid(XX$,1,4)+"+QUO+" "+QUO+"+H"+STR(AA)+"$+"+QUO+CO$+QUO+"+M"+STR(AA)+"$+"+QUO+CO$+QUO+"+S"+STR(AA)+"$+"+QUO+CO$+QUO
17022 EXECUTE "if d"+STR(AA)+"$="+QUO+QUO+" then "+DT$[AA]+"=0;goto 17026"
17025 EXECUTE DT$[AA]+"=FN%DTN(TMP"+STR(AA)+"$,"+QUO+"MMDDYYYY HH:MM:SS"+QUO+")"
17080 NEXT AA
17090 RETURN 
17100 ASSIGN_DATE:
17106 PRECISION 7
17110 FOR AA=1 TO 3
17120 EXECUTE "TM$=STR("+DT$[AA]+")"
17125 IF NUM(TM$)=0 THEN {
17126 DIM RE$(20); EXECUTE "D"+STR(AA)+"$="+QUO+QUO
17128 GOTO 17175 }
17130 RE$=FN%NTD$(NUM(TM$),"MMDDYYYY HH:MM:SS")
17140 EXECUTE "D"+STR(AA)+"$=RE$(5,4)+RE$(1,2)+RE$(3,2),RE$=RE$(10)"
17150 EXECUTE "H"+STR(AA)+"$=RE$(1,2)"
17160 EXECUTE "M"+STR(AA)+"$=RE$(4,2)"
17170 EXECUTE "S"+STR(AA)+"$=RE$(7,2)"
17180 NEXT AA
17190 RETURN 
