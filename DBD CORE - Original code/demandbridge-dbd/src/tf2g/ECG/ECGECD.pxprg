17100 GET_ORDER_NUM:
17110 PROCESS "ECGECD.O","../ECG/EC.EN",ORDER_NO$; IF NUL(ORDER_NO$) THEN RETURN 
17120 OPEN (HFN,ERR=*RETURN)"FS1"+%C$; FS1=LFO
17130 FIND (FS1,KEY=ORDER_NO$,DOM=MISSING_ORDER)FS1$
17140 WORKSHEET_NO$=MID(FS1$,289,10); IF NUL(WORKSHEET_NO$) THEN WORKSHEET_NO$=""; GOTO NOT_WEBEC_ORDER ELSE %WORKSHEET_NO$=WORKSHEET_NO$
17150 ! NEXT_ID=WORKSHEET_NO.CTL; REFRESH_FLG=1; GOTO FIND_REC
17160 _KEY$=WORKSHEET_NO$; READ (_FIL_NO,KEY=_KEY$,ERR=MISSING_WORKSHEET)
17170 _ENABLE_FLG=-1; GOSUB PROCESS_READ; GOSUB DISP_REC
17190 GOTO ORDER_NUM_DONE
17191 MISSING_ORDER: MSGBOX "No such order found!"; GOTO ORDER_NUM_DONE
17192 NOT_WEBEC_ORDER: MSGBOX "This is not a WebEC order!"; GOTO ORDER_NUM_DONE
17193 MISSING_WORKSHEET: MSGBOX "No matching worksheet found for given order!"; GOTO ORDER_NUM_DONE
17195 ORDER_NUM_DONE:CLOSE (FS1)
17199 RETURN 
