0010 REM "Remove OS1 pricing records if no OS0 records <OSGCLR>
0020 SETESC 9300; SETERR 9000
0035 REM "5.6 - 10/27/08 - 18.029166 - crg - SSP# 207651
0037 REM "207651-Utility to clear orphaned OS1 rec if no OS0 rec for it.
0040 REM "Copyright 2008 Demand Bridge, LLC.; Norcross, Georgia
0041 REM "        Licensed Software - All Rights Reserved.
0102 IF TCB(88)=0 THEN MSGBOX "You must be using Windx to access this program" ELSE PROCESS "OSGCLR","../OSG/OS.EN"
0103 IF TCB(13)=1 THEN RUN "ZMENU" ELSE EXIT 
0105 INIT:
0110 X0$="OSGCLR",X1$="Remove orphaned OS1 pricing records"
0120 DIM Z0$(80,"-")
0125 SAVE_GUI=%GUI; %GUI=1
0130 K0=20,K1=1
0135 C9=-1
0150 X0=-1,X2=-1
0200 REM "
0240 IF Q1$="" THEN CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0300 REM "IOLISTS
0310 IOLIST A$
0320 IOLIST B$
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01O OS1...  02OSOS1...  03O OS0...  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1; ON Z0 GOTO 0521,9900
0600 REM "
0620 GOSUB 8100
0630 IF NOT(NUL(Q1$)) THEN GOTO MAIN
0640 CALL "*WINGRP;ENABLE",BUTTON.GRP$
0690 REFRESH_FLG=1
0700 RETURN 
1000 REM "BEGIN MAIN PROCESS
1005 MAIN:
1010 K$=KEY(Z[1],END=3000); READ (Z[1],KEY=K$)IOL=0310
1020 C=C+1; IF MOD(C,T1)=1 THEN GOSUB 8150
1030 FIND (Z[3],KEY=MID(A$,1,10),DOM=*NEXT); GOTO RECORD_DONE ! If OS0 rec exists, skip this OS1 record
1040 REMOVE (Z[2],KEY=K$,ERR=*NEXT) ! No OS0 record, remove from OS1
1100 RECORD_DONE:
1200 REM "
1600 GOTO 1010
5000 REM "EOJ
5001 IF Q1$<>"" THEN GOTO END_PROC
5010 C=T; GOSUB 8150
5020 MSGBOX MSG("PROC_C"),MSG("FYI"),"!,tim=5"
5080 CMD_STR$="END"; REFRESH_FLG=1
5090 RETURN 
5095 REM "
8100 REM "Initialize counts
8110 CALL "ZZINFO",Z[1],T9,X3$,T,T0,K,B,D,S0,S1,F$
8120 T1=INT(T*.02); IF T1<=1 THEN T1=2; REM " set to 2% of total record,if 1 set to 2 to do mod correctly
8130 PROC_REC=T; REFRESH_FLG=1
8145 RETURN 
8150 REM "Call this each time to update the bar graph
8155 IF NUL(Q1$) THEN CALL "ZZBARG",X3$,"HG",7,2,20,T2,T,C
8195 RETURN 
8950 DEF FNS$(Z9$)=Z9$(1,POS("     "=Z9$+"     ")-1)
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9045 REM 
9050 ON Y7 GOTO 9060,9100,9800,9070,9090
9055 REM 
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9800
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9900 REM "END PROGRAM
9901 END_PROC:
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9920 CMD_STR$="END"
9925 %GUI=SAVE_GUI
9930 SETERR 9940; Q1$=A1$; EXIT 
9940 SETESC 9350
9950 ! RUN "ZMENU"
9990 EXIT 
9999 END 
