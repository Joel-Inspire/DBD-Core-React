13020 IF _CLR_FLG$="R" THEN GOE_NUMBER$=""
13030 IF _CLR_FLG$="R" THEN PO_CODE$=""
13040 PLANT_NAME$=""
13050 PLANT_ATTN$=""
13060 PLANT_ADDR1$=""
13070 PLANT_ADDR2$=""
13080 PLANT_CITY$=""
13090 PLANT_STATE$=""
13100 PLANT_ZIP_CODE$=""
13110 PRINT_PO$=""
13120 NEW_REPEAT$=""
13130 CONFIRMING$=""
13140 GROUPING_CUTOFF_DATE$=""
13150 PLANT_QUOTE_NUM$=""
13160 PLANT_QUOTE_DATE$=""
13170 PREV_JOB_NUM$=""
13180 PREV_JOB_DATE$=""
13190 THIS_JOB_NUMBER$=""
13200 THIS_JOB_DATE$=""
13210 JOB_STATUS$=""
13220 JOB_STATUS_DATE$=""
13230 PROOF_STATUS$=""
13240 PROOF_STATUS_DATE$=""
13250 SCHD_SHIP_DATE$=""
13260 CUST_SERV_REP$=""
