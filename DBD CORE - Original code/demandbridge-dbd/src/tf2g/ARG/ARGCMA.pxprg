13999 RETURN 
14000 ! 14000 - Enable/Disable groups - _ENABLE_FLG: 0=don't!, >1=corresponding key segment on, others off, -1=buttons and non-key fields on
14010 ENABLE_GROUPS:
14020 IF _ENABLE_FLG=0 THEN GOTO *RETURN
14030 IF _ENABLE_FLG=2 THEN CALL "*wingrp;Enable",KEY2.GRP$; NEXT_ID=CUST_CODE.CTL ELSE CALL "*wingrp;Disable",KEY2.GRP$
14040 IF _ENABLE_FLG=3 THEN CALL "*wingrp;Enable",KEY3.GRP$; NEXT_ID=CREDIT_MEMO_NUM.CTL ELSE CALL "*wingrp;Disable",KEY3.GRP$
14970 IF _ENABLE_FLG>0 THEN CALL "*wingrp;Disable",FIELDS.GRP$; CALL "*wingrp;Disable",BUTTONS.GRP$
14980 IF _ENABLE_FLG<0 THEN CALL "*wingrp;Enable",FIELDS.GRP$; CALL "*wingrp;Enable",BUTTONS.GRP$
14990 RETURN 
15000 QUERY:
15010 VAL$=""
15020 PROCESS "QRY_AT1","../ARG/AR.EN",VAL$
15030 CUSTOMER$=VAL$(1,10),CREDIT_MEMO_NUM$=VAL$(11),_KEY$=VAL$
15040 IF ID$="CUSTOMER$" THEN CALL "ZZWLKU;PARSE_CUST",CUSTOMER$,CUST_DIV$,CUST_CODE$; CUSTOMER$=CUST_DIV$+CUST_CODE$
15050 GOTO 1070
16000 DELETE_FROM_ARGEAA:
16010 ENTER _KEY$
16020 _AT1=HFN,_FILE_NAME$="AT1"+%C$; OPEN (_AT1,ERR=NO_RECORD)_FILE_NAME$
16030 READ (_AT1,KEY=_KEY$,DOM=NO_RECORD)
16040 MSGBOX MSG("ARGCMA_01"),MSG("CONFIRMING"),"?,YESNO",OPT$
16050 IF OPT$="NO" THEN EXIT 
16060 REMOVE (_AT1,KEY=_KEY$,ERR=*NEXT,DOM=*NEXT)
16070 NO_RECORD:
16080 CLOSE (_AT1,ERR=*NEXT)
16090 EXIT 
17000 SEL_INVOICE: REFRESH_FLG=1
17010 READ (_AR6,KEY=CUSTOMER$+ORIGINAL_INVOICE_NUM$,REC=AR6$,DOM=SEL_AR8) ! dbspt-88623 SSP#307286
17020 ORIGINAL_INVOICEDATE$=AR6.INV_DATE$
17030 RETURN 
17040 SEL_AR8:! dbspt-88623 SSP#307286
17050 READ (_AR8,KEY=CUSTOMER$+ORIGINAL_INVOICE_NUM$,REC=AR8$,DOM=NO_INVOICE) ! dbspt-88623 SSP#307286
17060 ORIGINAL_INVOICEDATE$=AR8.INV_DATE$
17070 RETURN 
17100 NO_INVOICE:
17110 ORIGINAL_INVOICE_NUM$="",NEXT_ID=ORIGINAL_INVOICE_NUM.CTL
17120 RETURN 
18000 SEL_ITEM: REFRESH_FLG=1
18010 READ (_IC0,KEY=CUSTOMER1$+ITEM_CODE$,DOM=READ_FM1)IC0$
18020 %ITEM_DESC1$=IC0$(21,40)
18030 RETURN 
18040 READ_FM1:
18050 READ (_FM1,KEY=CUSTOMER1$+ITEM_CODE$,DOM=*RETURN)FM1$ ! DSPT-88623 SSP#307286
18060 %ITEM_DESC1$=FM1$(42,40)
18070 RETURN 
18080 NO_REC: ITEM_CODE$="",NEXT_ID=ITEM_CODE.CTL; RETURN 
18100 QRY_FM1: VAL$=""
18110 PROCESS "QRY_FM1.SP","../FMG/FM.EN",VAL$
18120 IF VAL$="" THEN RETURN 
18130 REFRESH_FLG=1; NEXT_ID=QUANTITY_CREDITED.CTL,ITEM_CODE$=VAL$; GOTO 18050
