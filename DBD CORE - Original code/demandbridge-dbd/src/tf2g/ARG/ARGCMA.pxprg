10000 ! 10000 - Convert numeric values to string
10010 NUM_TO_STR:
10020 QUANTITY_CREDITED$=STR(QUANTITY_CREDITED)
10030 CREDIT_MEMO_TOTAL$=STR(CREDIT_MEMO_TOTAL)
10040 AT1_UNUSED_2$=STR(AT1_UNUSED_2)
10050 AT1_UNUSED_3$=STR(AT1_UNUSED_3)
10060 AT1_UNUSED_4$=STR(AT1_UNUSED_4)
10999 RETURN 
11000 ! 11000 - Convert numeric strings to number
11010 STR_TO_NUM:
11020 _X$="QUANTITY_CREDITED",_X=QUANTITY_CREDITED.CTL,QUANTITY_CREDITED=NUM(QUANTITY_CREDITED$,ERR=NON_NUMERIC)
11030 _X$="CREDIT_MEMO_TOTAL",_X=CREDIT_MEMO_TOTAL.CTL,CREDIT_MEMO_TOTAL=NUM(CREDIT_MEMO_TOTAL$,ERR=NON_NUMERIC)
11040 _X$="AT1_UNUSED_2",_X=AT1_UNUSED_2.CTL,AT1_UNUSED_2=NUM(AT1_UNUSED_2$,ERR=NON_NUMERIC)
