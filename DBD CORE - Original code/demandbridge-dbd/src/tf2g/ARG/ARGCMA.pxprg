11970 RETURN 
11980 NON_NUMERIC: MSGBOX _MSG_NON_NUMER$+_X$,_ERROR$,"!"
11990 NEXT_ID=_X
11999 EXIT 
12000 ! 12000 - Check if the required fields have data
12010 CHECK_REQD_FLDS:
12020 _W_FLG=1
12999 RETURN 
13000 ! 13000 - Clear the record fields
13010 CLEAR_FIELDS:
13020 IF _CLR_FLG$="R" THEN CUST_DIV$="",CUSTOMER$=""
13030 IF _CLR_FLG$="R" THEN CUST_CODE$=""
13040 IF _CLR_FLG$="R" THEN CREDIT_MEMO_NUM$=""
13050 CREDIT_MEMO_DATE$=""
13060 ORIGINAL_INVOICE_NUM$=""
13070 ORIGINAL_INVOICEDATE$=""
13080 ITEM_CUST_DIV$="",CUSTOMER1$=""
13090 ITEM_CUST_CODE$=""
13100 ITEM_CODE$=""
13110 EMPLOYEE_CODE$=""
13120 REASON_CODE$=""
13130 NOTES_LINE_1$=""
13140 NOTES_LINE_2$=""
13150 NOTES_LINE_3$=""
13160 AT1_UNUSED_1$="",%ITEM_DESC1$=""
13170 QUANTITY_CREDITED=0
13180 CREDIT_MEMO_TOTAL=0
13190 AT1_UNUSED_2=0
13200 AT1_UNUSED_3=0
13210 AT1_UNUSED_4=0
13980 GOSUB NUM_TO_STR
