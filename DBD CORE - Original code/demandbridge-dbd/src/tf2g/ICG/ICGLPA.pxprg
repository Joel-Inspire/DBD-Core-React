1015 IF ID$="SEQ_NUM" THEN SEQ_NUM$=PAD(SEQ_NUM$,2,0,"0")
1016 REM IF ID$="RECEIPT_DATE" THEN GOSUB VALIDATE_RECEIPT_DATE ! SSP217774 jdf
