3310 GOSUB INCORPORATE_CHGS
3320 WRT: NEW_BIN_LOC$=PAD(BIN_LOC$,3),BIN_LOC$=OLD_BIN_LOC$; IF NEW_QTY$<>"" THEN PRECISION 5; CHG=NUM(NEW_QTY$)/(NUM(QTY$)+NUM(NEW_QTY$)),TOTAL_COST=TOTAL_COST-(TOTAL_COST*CHG),TOTAL_COST$=STR(TOTAL_COST),TOTAL_FRT=TOTAL_FRT-(TOTAL_FRT*CHG),TOTAL_FRT$=STR(TOTAL_FRT),TF_STORAGE=TF_STORAGE-(TF_STORAGE*CHG),TF_STORAGE$=STR(TF_STORAGE),FINANCE=FINANCE-(FINANCE*CHG),FINANCE$=STR(FINANCE),TOTAL_COST=NUM(TOTAL_COST$),TOTAL_FRT=NUM(TOTAL_FRT$),QTY=NUM(QTY$),TF_STORAGE=NUM(TF_STORAGE$),FINANCE=NUM(FINANCE$),AA=NUM(OLD_TOTAL_COST$)-TOTAL_COST,BB=NUM(OLD_TOTAL_FRT$)-TOTAL_FRT,CC=NUM(OLD_STORAGE$)-TF_STORAGE,DD=NUM(OLD_FINANCE$)-FINANCE,TOTAL_COST=NUM(OLD_TOTAL_COST$)-AA,TOTAL_COST$=STR(TOTAL_COST$),TOTAL_FRT=NUM(OLD_TOTAL_FRT$)-BB,TOTAL_FRT$=STR(TOTAL_FRT),TF_STORAGE=NUM(OLD_STORAGE$)-CC,TF_STORAGE$=STR(TF_STORAGE),FINANCE=NUM(OLD_FINANCE$)-DD,FINANCE$=STR(FINANCE); PRECISION 2 ELSE BIN_LOC$=NEW_BIN_LOC$
3330 WRITE (_FIL_NO,KEY=_KEY$,ERR=WRITE_ERROR)
3340 READ DATA FROM REC(IOL(_FIL_NO,ERR=*NEXT)),REC=_ORIG$ TO IOL=IOL(_FIL_NO) ! Save record for review
3350 GOSUB NUM_TO_STR
3360 REFRESH_FLG=1
3365 IF NEW_QTY$<>"" THEN GOSUB CREATE_RECORD
3366 _CLR_FLG$="R"; GOSUB CLEAR_FIELDS; NEXT_ID=CUSTOMER.CTL,NEXT_FOLDER=FLDR.ICGLPA.1.CTL
3370 RETURN 
