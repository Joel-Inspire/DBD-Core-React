0180 _CUR_FN=HFN; OPEN (_CUR_FN)_FILE_NAME$
0185 _ZZPARM=HFN; OPEN (_ZZPARM)"ZZPARM"
0190 RETURN 
0200 OPEN_ERR: MSGBOX _MSG_FILOPNERR1$+QUO+_FILE_NAME$+QUO+SEP+_MSG_DIRECTORY$+LWD+SEP+_MSG_PREFIX$+PFX,MSG(ERR),"!"
