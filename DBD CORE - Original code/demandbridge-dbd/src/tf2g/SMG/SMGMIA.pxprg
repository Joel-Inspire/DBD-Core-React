1026 IF ID$="PO_LINE_NUM" THEN GOSUB FIND_ORDER_LINE; IF NOT(VALID_LINE) THEN MSGBOX ("INVALID LINE"); NEXT_ID=PO_NUMBER.CTL; RETURN ! SSP246002
1030 IF _ENABLE_FLG<0 THEN _KCNT=_KEY1,_ENABLE_FLG=_KEY1
