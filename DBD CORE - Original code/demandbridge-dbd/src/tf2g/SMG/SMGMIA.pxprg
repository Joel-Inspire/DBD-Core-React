23500 CALC_SMI_MU:! WO284739-IFP-add markup to whse shipments
23505 MU_TO_APPLY=SMI_MU; GOSUB CHECK_SMC_MU; IF SMC_MU=-99.99 THEN MU=0; GOTO APPLY_MU ELSE IF SMC_MU<>0 THEN MU_TO_APPLY=SMC_MU ! WO268886, check for customer override in SMC, if -99.99 then bypass markup else if non-zero then use it instead of system marKp setting
23510 IF FS1$(54,1)="B" THEN MU=1; GOTO APPLY_MU ! B type order, apply markup every time.
23515 ! if not B type, then read order lines and find a match for this vendor, with a PO code, then if special shipping check that and see if any to D-9000 type and if so then the markup applies.
23520 MU=0; READ (Z[3],KEY=A$(1,8),DOM=*NEXT)
23525 CHECK_FS2: FS2K$=KEY(Z[3],END=APPLY_MU); IF FS2K$(1,8)<>A$(1,8) THEN GOTO APPLY_MU
23530 READ (Z[3],KEY=FS2K$,DOM=CHECK_FS2)CHECK_FS2$
23535 IF CHECK_FS2$(9,1)=DIM(1) THEN GOTO CHECK_FS2 ! only if PO code
23540 IF CHECK_FS2$(18,1)<>"Y" THEN GOTO CHECK_FS2 ! only special shipping
23545 IF CHECK_FS2$(94,10)<>VENDOR$ THEN GOTO CHECK_FS2 ! match vendor
23550 ! have FS2 with PO code, spec shipping and match on vendor so check PO3 and see if any shipping to D-9000 types
23555 READ (Z[4],KEY=FS2K$(1,8)+DIM(1)+FS2K$(9,3),DOM=*NEXT)
23560 CHECK_PO3: PO3K$=KEY(Z[4],END=CHECK_FS2); IF PO3K$(1,12)<>FS2K$(1,8)+DIM(1)+FS2K$(9,3) THEN GOTO CHECK_FS2
23565 READ (Z[4],KEY=PO3K$,DOM=CHECK_PO3)CHECK_PO3$
23570 IF MID(CHECK_PO3$,11,2)="D9" THEN MU=1; GOTO APPLY_MU ! found special ship record for this line that is shipping to whse so apply markup
23575 GOTO CHECK_PO3
23580 ! 
23585 APPLY_MU:
23586 IF NOT(MU) THEN MARKUP_AMT$="0"
23587 MARKUP_AMT=NUM(MARKUP_AMT$),ORIG_FRT=NUM(ORIG_FRT$),TOTAL_FRT=NUM(TOTAL_FRT$)
23588 IF MU THEN MARKUP_AMT=ORIG_FRT*MU_TO_APPLY/100 ! calc markup and put in new MARKUP_AMT field, then add it to the TOTAL_FRT. WO268886, use MU_TO_APPLY rather than SMI_MU.
23589 CALC_TOT_FRT:
23590 TOTAL_FRT=ORIG_FRT+MARKUP_AMT ! calc markup and put in new MARKUP_AMT field, then add it to the TOTAL_FRT. WO268886, use MU_TO_APPLY rather than SMI_MU.
23591 MARKUP_AMT$=STR(MARKUP_AMT),ORIG_FRT$=STR(ORIG_FRT),TOTAL_FRT$=STR(TOTAL_FRT)
23592 REFRESH_FLG=1
23595 RETURN 
