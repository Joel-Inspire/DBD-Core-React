10150 REM "Parse the input file records
10155 PARSE_DATA:
10160 INFMT$=MID(FILE_FORMAT$,1,1)
10165 SKIP_REC=0,STATE$="",ZIP$="",COUNTY$="",CITY$=""
10195 SWITCH (INFMT$)
10199 ! 
10200 CASE "1" ! Veratax data
10210 IF NUL(MID(A$,3,5)) THEN SKIP_REC=1; BREAK ! Skip state header records
10220 STATE$=MID(A$,1,2),ZIP$=MID(A$,3,5),COUNTY$=MID(A$,64,25),CITY$=MID(A$,10,25)
10295 BREAK
10299 ! 
10300 CASE "2" ! CCH data
10310 STATE$=A$(59,2),ZIP$=A$(1,5),COUNTY$=A$(34,25),CITY$=A$(6,28)
10395 BREAK
10399 ! 
10400 CASE "3" ! TDS data
10410 XX=POS($09$=A$),ZIP$=A$(1,XX-1),A$=A$(XX+1)
10412 IF ZIP$="ZIP_CODE" THEN SKIP_REC=1; BREAK ! Skip header record
10415 XX=POS($09$=A$),STATE$=A$(1,XX-1),A$=A$(XX+1)
10420 XX=POS($09$=A$),COUNTY$=A$(1,XX-1),A$=A$(XX+1),COUNTY$=PAD(COUNTY$,25," ")
10425 XX=POS($09$=A$),CITY$=A$(1,XX-1),A$=A$(XX+1),CITY$=PAD(CITY$,25," ")
10495 BREAK
10499 ! 
10900 DEFAULT 
10949 ! 
10950 END SWITCH 
10959 ! 
10960 IF SKIP_REC THEN GOTO 10990
10965 DIM ZY6$(50); ZY6$(1,2)=STATE$,ZY6$(3,25)=COUNTY$; WRITE (Z[1],KEY=ZY6$(1,27))ZY6$
10970 DIM ZY7$(75); ZY7$(1,2)=STATE$,ZY7$(3,25)=COUNTY$,ZY7$(28,25)=CITY$; WRITE (Z[2],KEY=ZY7$(1,52))ZY7$
10980 SETTRACE PRINT STR(SKIP_REC)+"|"+STATE$+"|"+ZIP$+"|"+COUNTY$+"|"+CITY$
10995 RETURN 
