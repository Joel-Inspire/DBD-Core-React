14970 IF _ENABLE_FLG>0 THEN CALL "*wingrp;Disable",FIELDS.GRP$; CALL "*wingrp;Disable",BUTTONS.GRP$
14980 IF _ENABLE_FLG<0 THEN CALL "*wingrp;Enable",FIELDS.GRP$; CALL "*wingrp;Enable",BUTTONS.GRP$
14990 RETURN 
15000 ! 15000 - Determine the changes that have been made before writing
15010 DETERMINE_CHGS:
15020 DIM _CHG_VAR$(10,"N")
15030 IF _ORIG.STATE_CODE$=_CUR.STATE_CODE$ OR STATE_CODE$=_CUR.STATE_CODE$ THEN GOTO *NEXT ELSE IF _ORIG.STATE_CODE$=STATE_CODE$ THEN _CHG2$=_CHG2$+SEP+MSG("STATE")+" = "+_CUR.STATE_CODE$,_CHG_VAR$(1,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("STATE")+" = "+_CUR.STATE_CODE$
15040 IF _ORIG.STATE_DESC$=_CUR.STATE_DESC$ OR STATE_DESC$=_CUR.STATE_DESC$ THEN GOTO *NEXT ELSE IF _ORIG.STATE_DESC$=STATE_DESC$ THEN _CHG2$=_CHG2$+SEP+MSG("DESC")+" = "+_CUR.STATE_DESC$,_CHG_VAR$(2,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("DESC")+" = "+_CUR.STATE_DESC$
15050 IF _ORIG.ST_DOWNLOADED$=_CUR.ST_DOWNLOADED$ OR ST_DOWNLOADED$=_CUR.ST_DOWNLOADED$ THEN GOTO *NEXT ELSE IF _ORIG.ST_DOWNLOADED$=ST_DOWNLOADED$ THEN _CHG2$=_CHG2$+SEP+"Downloaded = "+_CUR.ST_DOWNLOADED$,_CHG_VAR$(3,1)="Y" ELSE _CHG1$=_CHG1$+SEP+"Downloaded = "+_CUR.ST_DOWNLOADED$
15060 IF _ORIG.USESALES_OR_USETAX$=_CUR.USESALES_OR_USETAX$ OR USESALES_OR_USETAX$=_CUR.USESALES_OR_USETAX$ THEN GOTO *NEXT ELSE IF _ORIG.USESALES_OR_USETAX$=USESALES_OR_USETAX$ THEN _CHG2$=_CHG2$+SEP+"Use Sales or Use Tax = "+_CUR.USESALES_OR_USETAX$,_CHG_VAR$(4,1)="Y" ELSE _CHG1$=_CHG1$+SEP+"Use Sales or Use Tax = "+_CUR.USESALES_OR_USETAX$
15070 IF _ORIG.ACCT_SALES_TAX$=_CUR.ACCT_SALES_TAX$ OR ACCT_SALES_TAX$=_CUR.ACCT_SALES_TAX$ THEN GOTO *NEXT ELSE IF _ORIG.ACCT_SALES_TAX$=ACCT_SALES_TAX$ THEN _CHG2$=_CHG2$+SEP+"Sales Tax Payable = "+_CUR.ACCT_SALES_TAX$,_CHG_VAR$(5,1)="Y" ELSE _CHG1$=_CHG1$+SEP+"Sales Tax Payable = "+_CUR.ACCT_SALES_TAX$
15080 IF _ORIG.ACCT_ACCR_SALES_TAX$=_CUR.ACCT_ACCR_SALES_TAX$ OR ACCT_ACCR_SALES_TAX$=_CUR.ACCT_ACCR_SALES_TAX$ THEN GOTO *NEXT ELSE IF _ORIG.ACCT_ACCR_SALES_TAX$=ACCT_ACCR_SALES_TAX$ THEN _CHG2$=_CHG2$+SEP+"Accrued Sales Tax = "+_CUR.ACCT_ACCR_SALES_TAX$,_CHG_VAR$(6,1)="Y" ELSE _CHG1$=_CHG1$+SEP+"Accrued Sales Tax = "+_CUR.ACCT_ACCR_SALES_TAX$
15090 IF _ORIG.TAX_FREIGHT$=_CUR.TAX_FREIGHT$ OR TAX_FREIGHT$=_CUR.TAX_FREIGHT$ THEN GOTO *NEXT ELSE IF _ORIG.TAX_FREIGHT$=TAX_FREIGHT$ THEN _CHG2$=_CHG2$+SEP+"Tax Freight = "+_CUR.TAX_FREIGHT$,_CHG_VAR$(7,1)="Y" ELSE _CHG1$=_CHG1$+SEP+"Tax Freight = "+_CUR.TAX_FREIGHT$
15100 IF _ORIG.TF_UNUSED$=_CUR.TF_UNUSED$ OR TF_UNUSED$=_CUR.TF_UNUSED$ THEN GOTO *NEXT ELSE IF _ORIG.TF_UNUSED$=TF_UNUSED$ THEN _CHG2$=_CHG2$+SEP+"Unused = "+_CUR.TF_UNUSED$,_CHG_VAR$(8,1)="Y" ELSE _CHG1$=_CHG1$+SEP+"Unused = "+_CUR.TF_UNUSED$
15990 RETURN 
16000 ! 16000 - Incorporate changes made by another user into the record
16010 INCORPORATE_CHGS:
16020 IF _CHG_VAR$(1,1)="Y" THEN STATE_CODE$=_CUR.STATE_CODE$
16030 IF _CHG_VAR$(2,1)="Y" THEN STATE_DESC$=_CUR.STATE_DESC$
