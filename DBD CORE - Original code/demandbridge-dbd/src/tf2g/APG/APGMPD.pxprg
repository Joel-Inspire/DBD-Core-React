0210 DIM APG$:"A000:C(6),A001:C(4),A002:C(2),A003:C(1),A004:C(1),A005:C(1),A006:C(1),A007:C(3),A008:C(1),A009:C(1),A010:C(10),A011:C(10),A012:C(2),A013:C(1),A014:C(3),A015:C(1),A016:C(1),A017:C(11),A018:C(1),A019:C(4),A020:C(1),A021:C(1),A022:C(1),A023:C(1),A024:C(1),A025:C(1),A026:C(4),A027:C(1),A028:C(1),A029:C(1),A030:C(1),A031:C(1),A032:C(1),A033:C(1)" ! SSP#231990
