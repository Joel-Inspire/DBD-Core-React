0041 ! "        Licensed Software - All Rights Reserved.
0050 PROCESS "APGUT1","../APG/AP.EN"
0060 EXIT 
0090 CLEAR ; SETERR 0100; ENTER X3$,X4$,Q0$,Q1$
0100 SETERR 9000
0105 INIT:
0110 X0$="AP2UT1",X1$="Rebuild Check Sort Utility",K9$="",K9=0
0120 DIM Z0$(80,"-")
0200 REM "
0240 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,-1,X1,X2; IF X1>0 THEN GOTO 9920
0245 IF X3$(66,1)>"2" THEN PRECISION NUM(X3$(66,1),ERR=0246)
0500 REM "FILES
0505 DIM Z[NUM(X3$(60,3))]
0510 Z$="01X APM...  02O APY...  "
0520 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,0; ON Z0 GOTO 0521,9900
0630 RETURN 
0900 REM 
0910 IF Q1$<>"*" THEN CALL "ZZPROM",".Y",X3$,Z,"Do you wish to rebuild the Check sort by check number file?","","",0
0920 ON Z GOTO 0921,9900
0950 PROCEED:
1000 REM "
1010 CALL "ZZINIT",STR(Z[2]:"00")
1100 REM 
1110 READ (Z[1],END=5000)A$
1115 ML_MESS.CTL'VALUE$=A$(1,4)+" "+A$(5,2)+" "+A$(7,3)+" "+A$(10,6)
1120 B$=A$(7,9)+A$(1,6)
1130 WRITE (Z[2],KEY=B$)
1190 GOTO 1100
5030 MSGBOX "Process complete!!",MSG("CONFIRMING"),"INFO,TIM=3"
5040 GOTO 9900
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9050 ON Y7 GOTO 9060,9100,9900,9070,9090
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 9900
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9900
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9800 REM "EXIT PROGRAM
9900 REM "END PROGRAM
9901 END_PROC: CMD_STR$="E"
9910 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
9920 SETERR 9940
9930 Q1$=A1$; EXIT 
9940 SETESC 9350
9950 RUN "ZMENU"
9999 END 
